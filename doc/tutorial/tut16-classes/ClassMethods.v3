class CmObject {
	def f: int;
	new(f) { }
	def add(a: int) -> int {
		return a + f;
	}
}
component ClassMethods {
	var o = CmObject.new(3);
	// "Class.method" can be used as a first class function
	var m: (CmObject, int) -> int = CmObject.add;
	// the receiver object is the first parameter
	var x = m(o, 2);
}
