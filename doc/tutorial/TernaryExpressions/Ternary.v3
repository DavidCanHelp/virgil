component Ternary {
	// with declared types
	var a: int = if(true, 17);	// == 17
	var b: int = if(false, 19);	// == 0 (default)
	var c: int = if(true, 16, 27);  // == 16
	var d: int = if(false, 17, 29); // == 29
	// with type inference
	var e = if(true, 37);	// == 37
	var f = if(false, 39);	// == 0 (default)
	var g = if(true, 46, 67);  // == 46
	var h = if(false, 47, 69); // == 69
}
