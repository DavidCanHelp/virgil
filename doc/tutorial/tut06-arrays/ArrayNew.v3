component ArrayNew {
	// create a new array with Array<Type>.new(length)
	var a: Array<int> = Array<int>.new(3);
	var b: Array<bool> = Array<bool>.new(7);
	// often <Type> can be inferred automatically
	var c: Array<bool> = Array.new(7);
	// or simply rely on local type inference
	var d = Array<bool>.new(7);
}
