def TEST = UnitTest.new("NumberParser", test);

def test() {
	n("-");

	var hex_buf = "0xx";
	var neg_buf = "-x";
	var pos_buf = "x";
	var alpha = "acbdefghijklmnopqrstuvABCDEFGHIJKLMNOPQRSTUVWXYZ";
	var symbol =  "`~!@#$%^&*()_=+[{]}|;:,<.>/?";
	var quote = "\\\'\"";
	for (s in [alpha, symbol, quote]) {
		for (ch in s) {
			neg_buf[1] = ch;
			n(neg_buf);
			pos_buf[0] = ch;
			n(pos_buf);
		}
	}

	for (ch in symbol) {
		hex_buf[2] = ch;
		n(hex_buf);
	}
	for (ch = 'G'; ch <= 'Z'; ch++) {
		hex_buf[2] = ch;
		n(hex_buf);
	}
	for (ch = 'g'; ch <= 'z'; ch++) {
		hex_buf[2] = ch;
		n(hex_buf);
	}

	testInts();
	testHexLongs();
	testDecLongs();
	testUnsigned();
	testTypes();
}

