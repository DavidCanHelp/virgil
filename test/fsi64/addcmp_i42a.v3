//@execute 0=true; 1=false; 2=true; 3=true; 4=false; 5=true; 6=true; 7=false; 8=true
def f(a: i42, b: i42) -> bool {
	return (a + b) > a;
}
def main(a: int) -> bool {
	match (a) {
		0 => return f(1, 1);
		1 => return f(1, i41.min);
		2 => return f(1, i41.max);
		3 => return f(i41.min, 1);
		4 => return f(i41.min, i41.min);
		5 => return f(i41.min, i41.max);
		6 => return f(i41.max, 1);
		7 => return f(i41.max, i41.min);
		8 => return f(i41.max, i41.max);
	}
	return true;
}
