//@execute 0=0; 1=4; 2=5; 3=6; 4=7; 5=!BoundsCheckException
type T {
	case I(val: int);
	case L(val: long);
	def get() -> int {
		match (this) {
			I(v): return v;
			L(v): return int.!(v);
		}
	}
}
def d: T;
def x = [d, T.L(4), T.L(5), T.I(6), T.I(7)];
def main(a: int) -> int {
	return x[a].get();
}