//@execute 0=!DivideByZeroException; 1=53; 2=53; 3=53; 7=53; -7=53; -2000=53; -2147483648=53
def inputs = [
	u32.!(0x00000000),
	u32.!(0x00000001),
	u32.!(0x00000002),
	u32.!(0x0000000F),
	u32.!(0x000000FF),
	u32.!(0x00000300),
	u32.!(0x00004000),
	u32.!(0x00050000),
	u32.!(0x00600000),
	u32.!(0x07000000),
	u32.!(0x10000000),
	u32.!(0x33000000),
	u32.!(0x55000000),
	u32.!(0x77770000),
	u32.!(0x80000000),
	u32.!(0x80000001),
	u32.!(0x80000005),
	u32.!(0x90000006),
	u32.!(0xa0000001),
	u32.!(0xb0000001),
	u32.!(0xf0000001),
	u32.!(0xff000001),
	u32.!(0xff000fff),
	u32.!(0xfffffffe),
	u32.!(0xffffffff)
];
def main(a: int) -> int {
	for (d in inputs) {
		var e = d / u32.!(a);
		var r = (u64.!(d) << 32) / (u64.!(u32.!(a)) << 32);
		if (e != r) return -109;
	}
	return 53;
}
