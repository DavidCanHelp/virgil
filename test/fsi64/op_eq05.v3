//@execute 0=true; 1=true; 2=true; 3=true; 4=true; 5=true; 6=true; 7=true; 8=true; 9=true; 10=true; 11=true; 12=true; 13=true; 14=true; 15=false
def main(a: int) -> bool {
	match (a) {
		0: return check(long.+);
		1: return check(long.-);
		2: return check(long.*);
		3: return check(long./);
		4: return check(long.%);
		5: return check(long.&);
		6: return check(long.|);
		7: return check(long.^);
		8: return check(long.<<);
		9: return check(long.>>);
		10: return check(long.>>);
		11: return check(long.<);
		12: return check(long.<=);
		13: return check(long.>);
		14: return check(long.>=);
	}
	return false;
}
def id<T>(a: T) -> T {
	return a;
}
def check<T>(a: T) -> bool {
	return a == id(a);
}
