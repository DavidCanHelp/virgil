//@execute 0=12; 1=13; 2=14; 3=15; 4=16; 5=17; 6=18; 7=19; 8=20; 9=21; 10=!BoundsCheckException
def ops = [
	(long.+,   0xAABC666666656666),
	(long.-,   0xAABB33557798BBDE),
	(long.*,   0xE82C51F5CDFCD708),
	(long./,   0),
	(long.%,   0),
	(long.&,   0),
	(long.|,   0),
	(long.^,   0),
	(long.#<<, 0),
	(long.#>>, 0)
];
def main(a: int) -> int {
	def x = 0xAABBCCDDEEFF1122;
	def y = 0x0000998877665544;
	var op = ops(a);
	if (op.0(x, y) != op.1) return -100 - a;
	return 12 + a;
}
