//@execute 0=true; 1=false; 2=true; 3=true; 4=false; 5=true; 6=true; 7=false; 8=true
def f(a: i64, b: i64) -> bool {
	return (a + b) > a;
}
def x = 1L;
def y = long.!(i63.min);
def z = long.!(i63.max);
def main(a: int) -> bool {
	match (a) {
		0: return (x + x) > x;
		1: return (x + y) > x;
		2: return (x + z) > x;
		3: return (y + x) > y;
		4: return (y + y) > y;
		5: return (y + z) > y;
		6: return (z + x) > z;
		7: return (z + y) > z;
		8: return (z + z) > z;
	}
	return true;
}
