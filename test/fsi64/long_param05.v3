//@execute 0=1; 1=1; 2=3; 3=0; 4=5; 5=1; 6=7; 7=0; 8=9; 9=1; 10=11; 11=0; 12=13; 13=1; 14=15; 15=0
def main(arg: int) -> int {
	return int.!(big(arg, 1, true, 3, false, 5, true, 7, false, 9, true, 11, false, 13, true, 15, false));
}
def big(w: long, p0:  long, p1:  bool, p2:  long, p3:  bool, p4:  long, p5:  bool, p6:  long, p7:  bool, 
		   p8:  long, p9:  bool, p10: long, p11: bool, p12: long, p13: bool, p14: long, p15: bool) -> long {
	if (w == 0) return p0;
	if (w == 1) return i(p1);
	if (w == 2) return p2;
	if (w == 3) return i(p3);
	if (w == 4) return p4;
	if (w == 5) return i(p5);
	if (w == 6) return p6;
	if (w == 7) return i(p7);
	if (w == 8) return p8;
	if (w == 9) return i(p9);
	if (w == 10) return p10;
	if (w == 11) return i(p11);
	if (w == 12) return p12;
	if (w == 13) return i(p13);
	if (w == 14) return p14;
	if (w == 15) return i(p15);
	return 42;
}
def i(b: bool) -> long {
	if (b) return 1;
	return 0;
}
