//@execute 0=true; 1=false; 2=true; 3=true; 4=false; 5=true; 6=true; 7=false; 8=true; 9=true
def f(a: i44, b: i44) -> bool {
	return (a + b) > a;
}
var x: i44 = 1;
var y: i44 = -3000000000;
var z: i44 = 3000000000;
def main(a: int) -> bool {
	match (a) {
		0: return f(x, x);
		1: return f(x, y);
		2: return f(x, z);
		3: return f(y, x);
		4: return f(y, y);
		5: return f(y, z);
		6: return f(z, x);
		7: return f(z, y);
		8: return f(z, z);
	}
	return true;
}
