//@execute 0=42; 1=42; 2=42; 3=42; 4=42; 5=42; 6=42; 7=42; 8=42; 9=42; 10=42; 11=42; 12=42; 13=42; 14=!BoundsCheckException
def length = 14;
def x(a: int) -> long {
	match (a) {
		0: return -2000000000000;
		1: return -1000000000000;
		2: return -500000000;
		3: return -500000;
		4: return -300;
		5: return -1;
		6: return 0;
		7: return 1;
		8: return 200;
		9: return 40000;
		10: return 7000000;
		11: return 8000000000;
		12: return 900000000000;
		13: return 5000000000000;
	}
	return [0L][1];
}
def main(a: int) -> int {
	for (i < length) {
		if (a < i) {
			if (x(a) < x(i)) continue;
			return -20 - i;
		} else {
			if (x(a) < x(i)) return -20 - i;
		}
	}
	return 42;
}
