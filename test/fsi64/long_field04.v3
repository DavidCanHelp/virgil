var dx = 0xE20C4E3040E64B00;
var dy = 0xD09A67B179DFC000;
var dz = 0x9AE4AA65D9CC0180;

var gx: long, gy: long, gz: long;
def main(a: int) -> int {
	gx = dx + a;
	gy = dy + a;
	gz = dz + a;
	match (a % 12) {
		0 =>  return int.!(u16.!(gx >> 0));
