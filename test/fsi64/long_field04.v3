		7 =>  return int.!(u16.!(gy >> 48));
		8 =>  return int.!(u16.!(gz >> 0));
		9 =>  return int.!(u16.!(gz >> 16));
		10 => return int.!(u16.!(gz >> 32));
		11 => return int.!(u16.!(gz >> 48));
	}
	return 13;
}
