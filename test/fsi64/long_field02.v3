//@execute 0=8; 1=7; 2=6; 3=5; 4=4; 5=3; 6=2; 7=1; 8=136; 9=119; 10=102; 11=85; 12=68; 13=51; 14=34; 15=17
class lf02(x: long) {
	def get(b: int) -> byte {
		match (b) {
			0 =>  return byte.view(x >> 0);
			1 =>  return byte.view(x >> 8);
			2 =>  return byte.view(x >> 16);
			3 =>  return byte.view(x >> 24);
			4 =>  return byte.view(x >> 32);
			5 =>  return byte.view(x >> 40);
			6 =>  return byte.view(x >> 48);
			7 =>  return byte.view(x >> 56);
		}
		return 0;
	}
}
def x = lf02.new(0x0102030405060708L);
def y = lf02.new(0x1122334455667788L);
def main(a: int) -> int {
	if (a < 8) return x.get(a);
	return y.get(a - 8);
}
