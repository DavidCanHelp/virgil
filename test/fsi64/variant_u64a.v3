//@execute (0,0)=45; (0,1)=220; (1,0)=135; (1,1)=660
type T {
	case Add(x: u64, y: u64);
	case Sub(x: u64, y: u64);
}
def exec(t: T) -> u64 {
	match (t) {
		Add(x, y): return x + y;
		Sub(x, y): return x - y;
	}
}
def x = [T.Add(0, 5), T.Add(1, 14)];
def y = [T.Sub(100, 91), T.Sub(144, 100)];
def main(a: int, b: int) -> int {
	return int.!(exec(x[a]) * exec(y[b]));
}
