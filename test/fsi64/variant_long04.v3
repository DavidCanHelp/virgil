//@execute 0=0; 1=4; 2=5; 3=6; 4=7; 5=!BoundsCheckException
type T {
	case L(nonce: int, val: long);
	case I(nonce: int, val: int);
	def get() -> int {
		match (this) {
			L(n, v): return n + int.!(v);
			I(n, v): return n + v;
		}
	}
}
def main(a: int) -> int {
	def d: T;
	def x = [d, T.L(a, 4 - a), T.L(a, 5 - a), T.I(a, 6 - a), T.I(a, 7 - a)];
	return x(a).get();
}
