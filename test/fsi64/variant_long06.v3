//@execute (0,0)=45; (0,1)=220; (1,0)=135; (1,1)=660
type T {
	case Add(x: long, y: long);
	case Sub(x: long, y: long);
}
def exec(t: T) -> long {
	match (t) {
		Add(x, y): return x + y;
		Sub(x, y): return x - y;
	}
}
def x = [T.Add(4, 1), T.Add(7, 8)];
def y = [T.Sub(11, 2), T.Sub(99, 55)];
def main(a: int, b: int) -> int {
	return int.!(exec(x[a]) * exec(y[b]));
}
