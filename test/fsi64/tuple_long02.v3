//@execute 0=0; 1=2; 2=6; 3=12; 4=4; 5=10; 6=18; 7=28
def main(a: int) -> int {
	var t = do(a);
	match (a & 3) {
		0 => return int.view(t.0);
		1 => return int.view(t.0 >> 32);
		2 => return int.view(t.1);
		3 => return int.view(t.1 >> 32);
	}
	return -1;
}
def do(a: int) -> (long, long) {
	return (0x0000000200000001L * a, 0x0000000400000003L * a);
}
