//@execute 0=true; 1=false; 2=true; 3=true; 4=false; 5=true; 6=true; 7=false; 8=true
def f(a: i44, b: i44) -> bool {
	return (a + b) > a;
}
def main(a: int) -> bool {
	match (a) {
		0: return f(1, 1);
		1: return f(1, i43.min);
		2: return f(1, i43.max);
		3: return f(i43.min, 1);
		4: return f(i43.min, i43.min);
		5: return f(i43.min, i43.max);
		6: return f(i43.max, 1);
		7: return f(i43.max, i43.min);
		8: return f(i43.max, i43.max);
	}
	return true;
}
