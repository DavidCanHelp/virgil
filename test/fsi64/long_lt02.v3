//@execute 0=42; 1=42; 2=42; 3=42; 4=42; 5=42; 6=42; 7=42; 8=42; 9=42; 10=42; 11=42; 12=42; 13=42; 14=!BoundsCheckException
def x = [
	-2000000000000,
	-1000000000000,
	-500000000,
	-500000,
	-300,
	-1,
	0,
	1,
	200,
	40000,
	7000000,
	8000000000,
	900000000000,
	5000000000000
];
def main(a: int) -> int {
	for (i < x.length) {
		var is = x(a) < x(i);
		if (a < i) {
			if (is) continue;
			return -20 - i;
		} else {
			if (is) return -20 - i;
		}
	}
	return 42;
}
