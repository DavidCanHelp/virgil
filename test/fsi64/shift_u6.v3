//@execute 0=11; 1=12; 2=13; 3=14; 4=15
def values = [
	1L,
	0x13,
	0x73,
	0x10000073,
	0x1002000000073,
	0x1004000000000073
];
def main(a: int) -> int {
	for (y in values) {
		var x = values[a];
		var expect = x << byte.view(y);
		if (expect != emu_shift(x, y)) return -1;
	}
	return 11 + a;
}
def emu_shift(x: long, y: long) -> long {
	var shift = u6.view(y);
	return if(y == shift, x << shift);
}
