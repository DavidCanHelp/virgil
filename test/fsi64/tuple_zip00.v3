def main() -> int {
	var r = zip([2L, 3L], [-1L, -2L]);
	return int.!(sum(r));
}
def zip<A, B>(a: Array<A>, b: Array<B>) -> Array<(A, B)> {
	var r = Array<(A, B)>.new(a.length);
	for (i < r.length) r(i) = (a(i), b(i));
	return r;
}
def sum(a: Array<(long, long)>) -> long {
	var sum = 0L;
	for (i < a.length) sum = sum + a(i).0 + a(i).1;
	return sum;
}
