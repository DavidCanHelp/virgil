//@execute 1=28; 2=28; 3=28; 4=28; 5=28; 6=28; 7=28
def vals: Array<i5> = [
	0b10001,
	0b11000,
	0b11100,
	0b11110,
	0b11111,
	0b11111,
	0b11111,
	0b11111,
	0b11111,
	0b11111
];
def main(a: byte) -> int {
	for (x = a; x < vals.length; x = x + a) {
		var n = vals(x - a) >> a;
		if (n != vals(x)) return -38;
	}
	return 28;
}
