//@execute 0=0; 1=3; 2=3; 3=3; 4=3; 5=3; 6=3; 7=3; 8=3; 9=3; 10=0
def main(a: int) -> u32 {
	var r: u32;
	match (a) {
		0 => r = u32.!(0x1) % u32.!(0x1);
		1 => r = u32.!(0x13) % u32.!(0x10);
		2 => r = u32.!(0x103) % u32.!(0x100);
		3 => r = u32.!(0x1003) % u32.!(0x1000);
		4 => r = u32.!(0x10003) % u32.!(0x10000);
		5 => r = u32.!(0x100003) % u32.!(0x100000);
		6 => r = u32.!(0x1000003) % u32.!(0x1000000);
		7 => r = u32.!(0x10000003) % u32.!(0x10000000);
		8 => r = u32.!(0x20000003) % u32.!(0x20000000);
		9 => r = u32.!(0x40000003) % u32.!(0x40000000);
		9 => r = u32.!(0x80000003) % u32.!(0x80000000);
	}
	return r;
}
