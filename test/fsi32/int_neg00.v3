//@execute 0=88; 1=88; 2=88; 3=88; 15=88; 6=88; 16=88
def main(a: int) -> int {
	for (i: byte < 32) {
		var val = a << i;
		var a1 = i1.!(val);
		if (~a1 != (-1 ^ a1)) return -101;

		var a5 = i5.!(val);
		if (~a5 != (-1 ^ a5)) return -105;

		var a7 = i7.!(val);
		if (~a7 != (-1 ^ a7)) return -105;

		var a8 = i8.!(val);
		if (~a8 != (-1 ^ a8)) return -108;

		var a9 = i9.!(val);
		if (~a9 != (-1 ^ a9)) return -108;

		var a15 = i15.!(val);
		if (~a15 != (-1 ^ a15)) return -115;

		var a16 = i16.!(val);
		if (~a16 != (-1 ^ a16)) return -116;

		var a17 = i17.!(val);
		if (~a17 != (-1 ^ a17)) return -116;

		var a31 = i31.!(val);
		if (~a31 != (-1 ^ a31)) return -116;
	}
	return 88;
}
