//@execute 1=6; 2=6; 3=6; 4=6; 5=6; 6=6; 7=6; 8=6; 9=6; 10=6
def vals: Array<i8> = [
	0b10100111,
	0b01010011,
	0b00101001,
	0b00010100,
	0b00001010,
	0b00000101,
	0b00000010,
	0b00000001,
	0b00000000,
	0b00000000,
	0b00000000,
	0b00000000,
	0b00000000
];
def main(a: byte) -> int {
	for (x = a; x < vals.length; x = x + a) {
		var n = vals(x - a) >>> a;
		if (n != vals(x)) return -6;
	}
	return 6;
}
