//@execute 1=7
def vals: Array<i15> = [
	0b100110101000101,
	0b010011010100010,
	0b001001101010001,
	0b000100110101000,
	0b000010011010100,
	0b000001001101010,
	0b000000100110101,
	0b000000010011010,
	0b000000001001101,
	0b000000000100110,
	0b000000000010011,
	0b000000000001001,
	0b000000000000100,
	0b000000000000010,
	0b000000000000001,
	0b000000000000000
];
def main(a: byte) -> int {
	for (x = a; x < vals.length; x = x + a) {
		var p = vals[x - a];
		match (a) {
			1 => p >>>= 1;
			2 => p >>>= 2;
			3 => p >>>= 3;
			4 => p >>>= 4;
			_ => p >>>= a;
		}
		if (p != vals[x]) return -7;
	}
	return 7;
}
