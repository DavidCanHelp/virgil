//@execute 1=6; 2=6; 3=6; 4=6; 5=6; 6=6; 7=6; 8=6; 9=6; 10=6; 11=6
def vals: Array<i9> = [
	0b100001011,
	0b010000101,
	0b001000010,
	0b000100001,
	0b000010000,
	0b000001000,
	0b000000100,
	0b000000010,
	0b000000001,
	0b000000000,
	0b000000000,
	0b000000000,
	0b000000000,
	0b000000000
];
def main(a: byte) -> int {
	for (x = a; x < vals.length; x = x + a) {
		var n = vals(x - a) >>> a;
		if (n != vals(x)) return -6;
	}
	return 6;
}
