//@execute 0=0; 1=3; 2=3; 3=3; 4=3; 5=3; 6=3; 7=3; 8=3; 9=3; 10=0
def main(a: int) -> u32 {
	var r: u32;
	match (a) {
		0 => r = (0x1u) % (0x1u);
		1 => r = (0x13u) % (0x10u);
		2 => r = (0x103u) % (0x100u);
		3 => r = (0x1003u) % (0x1000u);
		4 => r = (0x10003u) % (0x10000u);
		5 => r = (0x100003u) % (0x100000u);
		6 => r = (0x1000003u) % (0x1000000u);
		7 => r = (0x10000003u) % (0x10000000u);
		8 => r = (0x20000003u) % (0x20000000u);
		9 => r = (0x40000003u) % (0x40000000u);
	}
	return r;
}
