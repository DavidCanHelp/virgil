//@execute 1=6; 2=6; 3=6; 4=6; 5=6; 6=6; 7=6; 8=6; 9=6; 10=6; 11=6; 12=6; 13=6; 14=6; 15=6; 16=6; 17=6
def vals: Array<i15> = [
	0b011101010101110,
	0b111010101011100,
	0b110101010111000,
	0b101010101110000,
	0b010101011100000,
	0b101010111000000,
	0b010101110000000,
	0b101011100000000,
	0b010111000000000,
	0b101110000000000,
	0b011100000000000,
	0b111000000000000,
	0b110000000000000,
	0b100000000000000,
	0b000000000000000,
	0b000000000000000,
	0b000000000000000,
	0b000000000000000,
	0b000000000000000,
	0b000000000000000
];
def main(a: byte) -> int {
	for (x = a; x < vals.length; x = x + a) {
		var n = vals(x - a) << a;
		if (n != vals(x)) return -6;
	}
	return 6;
}
