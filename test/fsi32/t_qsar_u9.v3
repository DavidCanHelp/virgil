//@execute 1=2; 2=2; 3=2; 4=2; 5=2; 6=2; 7=2; 8=2; 9=2; 10=2; 11=2
def vals: Array<u9> = [
	0b101100100,
	0b010110010,
	0b001011001,
	0b000101100,
	0b000010110,
	0b000001011,
	0b000000101,
	0b000000010,
	0b000000001,
	0b000000000,
	0b000000000,
	0b000000000,
	0b000000000,
	0b000000000
];
def main(a: byte) -> int {
	for (x = a; x < vals.length; x = x + a) {
		var n = vals[x - a] >> a;
		if (n != vals[x]) return -2;
	}
	return 2;
}
