//@execute 0=0; 1=3; 2=3; 3=3; 4=3; 5=3; 6=3; 7=3; 8=3; 9=3; 10=0
def u32_mod(a: int, b: int) -> int {
	return int.!(u32.!(a) % u32.!(b));
}
def main(a: int) -> int {
	var r = 0;
	match (a) {
		0 => r = u32_mod(0x1, 0x1);
		1 => r = u32_mod(0x13, 0x10);
		2 => r = u32_mod(0x103, 0x100);
		3 => r = u32_mod(0x1003, 0x1000);
		4 => r = u32_mod(0x10003, 0x10000);
		5 => r = u32_mod(0x100003, 0x100000);
		6 => r = u32_mod(0x1000003, 0x1000000);
		7 => r = u32_mod(0x10000003, 0x10000000);
		8 => r = u32_mod(0x20000003, 0x20000000);
		9 => r = u32_mod(0x40000003, 0x40000000);
		9 => r = u32_mod(0x80000003, 0x80000000);
	}
	return r;
}
