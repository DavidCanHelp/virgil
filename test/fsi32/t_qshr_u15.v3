//@execute 1=6; 2=6; 3=6; 4=6; 5=6; 6=6; 7=6; 8=6; 9=6; 10=6; 11=6; 12=6; 13=6; 14=6; 15=6; 16=6; 17=6
def vals: Array<u15> = [
	0b100000000010101,
	0b010000000001010,
	0b001000000000101,
	0b000100000000010,
	0b000010000000001,
	0b000001000000000,
	0b000000100000000,
	0b000000010000000,
	0b000000001000000,
	0b000000000100000,
	0b000000000010000,
	0b000000000001000,
	0b000000000000100,
	0b000000000000010,
	0b000000000000001,
	0b000000000000000,
	0b000000000000000,
	0b000000000000000,
	0b000000000000000,
	0b000000000000000
];
def main(a: byte) -> int {
	for (x = a; x < vals.length; x = x + a) {
		var n = vals[x - a] >>> a;
		if (n != vals[x]) return -6;
	}
	return 6;
}
