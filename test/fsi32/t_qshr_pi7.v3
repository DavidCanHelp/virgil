//@execute 1=28; 2=28; 3=28; 4=28; 5=28; 6=28; 7=28; 8=28; 9=28
def vals: Array<i7> = [
	0b0101001,
	0b0010100,
	0b0001010,
	0b0000101,
	0b0000010,
	0b0000001,
	0b0000000,
	0b0000000,
	0b0000000,
	0b0000000,
	0b0000000,
	0b0000000
];
def main(a: byte) -> int {
	for (x = a; x < vals.length; x = x + a) {
		var n = vals(x - a) >>> a;
		if (n != vals(x)) return -12;
	}
	return 28;
}
