//@execute 1=22; 2=22; 3=22; 4=22; 5=22; 6=22; 7=22; 8=22; 9=22; 10=22; 11=22; 12=22; 13=22; 14=22; 15=22; 16=22; 17=22
def vals: Array<i15> = [
	0b011110011110111,
	0b001111001111011,
	0b000111100111101,
	0b000011110011110,
	0b000001111001111,
	0b000000111100111,
	0b000000011110011,
	0b000000001111001,
	0b000000000111100,
	0b000000000011110,
	0b000000000001111,
	0b000000000000111,
	0b000000000000011,
	0b000000000000001,
	0b000000000000000,
	0b000000000000000,
	0b000000000000000,
	0b000000000000000,
	0b000000000000000,
	0b000000000000000
];
def main(a: byte) -> int {
	for (x = a; x < vals.length; x = x + a) {
		var n = vals(x - a) >> a;
		if (n != vals(x)) return -80;
	}
	return 22;
}
