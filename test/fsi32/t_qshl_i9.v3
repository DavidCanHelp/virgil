//@execute 1=2; 2=2; 3=2; 4=2; 5=2; 6=2; 7=2; 8=2; 9=2; 10=2; 11=2
def vals: Array<i9> = [
	0b010100011,
	0b101000110,
	0b010001100,
	0b100011000,
	0b000110000,
	0b001100000,
	0b011000000,
	0b110000000,
	0b100000000,
	0b000000000,
	0b000000000,
	0b000000000,
	0b000000000,
	0b000000000
];
def main(a: byte) -> int {
	for (x = a; x < vals.length; x = x + a) {
		var n = vals(x - a) << a;
		if (n != vals(x)) return -2;
	}
	return 2;
}
