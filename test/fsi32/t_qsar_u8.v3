//@execute 1=6; 2=6; 3=6; 4=6; 5=6; 6=6; 7=6; 8=6; 9=6; 10=6
def vals: Array<u8> = [
	0b10001110,
	0b01000111,
	0b00100011,
	0b00010001,
	0b00001000,
	0b00000100,
	0b00000010,
	0b00000001,
	0b00000000,
	0b00000000,
	0b00000000,
	0b00000000,
	0b00000000
];
def main(a: byte) -> int {
	for (x = a; x < vals.length; x = x + a) {
		var n = vals[x - a] >> a;
		if (n != vals[x]) return -6;
	}
	return 6;
}
