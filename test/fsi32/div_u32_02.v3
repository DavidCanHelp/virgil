//@execute 0=2; 1=2; 2=2; 3=2; 4=2; 5=2; 6=2; 7=2; 8=2; 9=2; 10=0
def main(a: int) -> u32 {
	var r: u32;
	match (a) {
		0 => r = u32.!(0x2) / u32.!(0x1);
		1 => r = u32.!(0x23) / u32.!(0x10);
		2 => r = u32.!(0x203) / u32.!(0x100);
		3 => r = u32.!(0x2003) / u32.!(0x1000);
		4 => r = u32.!(0x20003) / u32.!(0x10000);
		5 => r = u32.!(0x200003) / u32.!(0x100000);
		6 => r = u32.!(0x2000003) / u32.!(0x1000000);
		7 => r = u32.!(0x20000003) / u32.!(0x10000000);
		8 => r = u32.!(0x40000003) / u32.!(0x20000000);
		9 => r = u32.!(0x80000003) / u32.!(0x40000000);
	}
	return r;
}
