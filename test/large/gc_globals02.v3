//@execute 0=0; 1=3; 5=-625739557
//@heap-size=4000
component gc_globals02 {
	var o: Array<int>;
	var p: Array<int>;
	var q: Array<int>;
	var hash: int;
	def main(a: int) -> int {
		o = p = q = Array<int>.new(3);
		hash = 0;
		for (i = 0; i < a; i++) {
			var x = Array<int>.new(hash & 0xFF);
			for (j = 0; j < o.length && j < x.length; j++) {
				x[j] = o[j] + hash;
				hash = hash * 33 + x.length + o.length;
			}
			q = p;
			p = o;
			o = x;
			hash = hash + 3;
		}
		return hash;
	}
}
