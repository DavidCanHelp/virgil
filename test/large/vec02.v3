//@execute (11, 128)=9; (3, 16)=1; (13, 256)=13; (1, 257)=!BoundsCheckException
def a = dup(11, 256);
def b = dup(12, 256);
def c = dup(13, 256);
def main(x: int, var n: int) -> int {
	for (i = 0; i < n; i++) {
		b[i++] = x;
	}

	var j = 0;
	while (n-- > 0) {
		a[j] = b[j] & c[j]; 
		j++;
	}
	return a[0];
}
def dup(x: int, num: int) -> Array<int> {
	var r = Array<int>.new(num);
	for (i = 0; i < r.length; i++) {
		r[i] = x;
	}
	return r;
}
