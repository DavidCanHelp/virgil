component gc_bigobj2 {
	def main(a: int) -> int {
		var x = gc_bigobj2a.new(a + 1);
		var sx = x.sum();
		var y = gc_bigobj2a.new(a + 2);
		var sy = y.sum();
		if (sx != x.sum()) return -1;
		if (sy != y.sum()) return -2;
		return sx + sy;
	}
}
