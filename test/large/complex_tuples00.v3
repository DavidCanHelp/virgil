//@execute 0=0; 1=1; 2=2; 3=3; 4=4
def KB: byte = 44;
def KI: int = 45;
def KL: long = -46;
def KZ: bool = true;

def f<A>(a: A) -> A {
	if (A.?(KB)) a = A.!(KB);
	if (A.?(KI)) a = A.!(KI);
	if (A.?(KL)) a = A.!(KL);
	var t = (KI, KB);
	if (A.?(t)) a = A.!(t);
	var w = (KI, KI, KB);
	if (A.?(w)) a = A.!(w);
	return a;
}
def testBase<A, B>() {
	var a: A, b: B;
	f(a);
	f(b);
	f((a, b));
	f((b, a));
	f((a, a, b, b));
	f((a, b, b, a));
}
def test<A, B>() {
	testBase<A, B>();
	testBase<B, A>();
	testBase<A, A>();
	testBase<B, B>();
	testBase<A, void>();
	testBase<void, B>();
	testBase<(A, B), (B, A)>();
	testBase<void, (A, A, B)>();
	testBase<(A, void), (void, B)>();
	testBase<(A, A, B), (B, A, B)>();
}
def main(a: int) -> int {
	testBase<void, void>();
	match (a) {
		0: test<int, byte>();
		1: test<int, bool>();
		2: test<int, long>();
		3: test<byte, bool>();
		4: test<byte, long>();
	}
	return a;
}