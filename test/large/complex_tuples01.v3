//@execute 0=0; 1=1; 2=2; 3=3; 4=4
//@heap-size=2000
def KB: byte = 44;
def KI: int = 45;
def KL: long = -46;
def KZ: bool = true;

class Base { }
class Some<T>(val: T) extends Base { }

def vals: Array<Base> = [Some.new(KB), Some.new(KI), Some.new(KL), Some.new(KZ)];

def f<A>(a: A) -> A {
	for (x in vals) if (Some<A>.?(x)) return Some<A>.!(x).val;
	var extr: Array<Base> = [Some.new(a), Some.new(a)];
	for (x in extr) if (Some<A>.?(x)) return Some<A>.!(x).val;
	return a;
}
def testBase<A, B>() {
	var a: A, b: B;
	f(a);
	f(b);
	f((a, b));
	f((b, a));
	f((a, a, b, b));
	f((a, b, b, a));
}
def test<A, B>() {
	testBase<A, B>();
	testBase<B, A>();
	testBase<A, A>();
	testBase<B, B>();
	testBase<A, void>();
	testBase<void, B>();
	testBase<(A, B), (B, A)>();
	testBase<void, (A, A, B)>();
	testBase<(A, void), (void, B)>();
	testBase<(A, A, B), (B, A, B)>();
}
def main(a: int) -> int {
	testBase<void, void>();
	match (a) {
		0: test<int, byte>();
		1: test<int, bool>();
		2: test<int, long>();
		3: test<byte, bool>();
		4: test<byte, long>();
	}
	return a;
}