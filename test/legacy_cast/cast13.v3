//@execute 0=42; 1=42; 2=42; 3=42; 4=42; 5=42
def main(arg: int) -> int {
	var v: void;
	var f: void;
	if (arg == 1) f = cast('a');
	if (arg == 2) f = cast(0);
	if (arg == 3) f = cast("a");
	if (arg == 4) f = cast(true);
	f = void.!(v);
	return 42;
}
def cast<T>(a: T) -> void { return void.!(a); }

