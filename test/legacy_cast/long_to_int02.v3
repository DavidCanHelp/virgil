//@execute 0=0; 1=-1; 2=2; 3=100; 4=-128; 5=-9999; 6=10009; 7=10000000; 8=2147483647; 9=-2147483648; 10=!BoundsCheckException
def x: Array<long> = [
	0L		+         0x100000000,
	-1L		+         0x200000000,
	2L		+         0xA00000000,
	100L		+       0xFFF00000000,
	-128L		+  0x1234567800000000,
	-9999L		+  0xabcdef9900000000,
	10009L		+  0x6829734100000000,
	10000000L	+  0x8217837700000000,
	2147483647L	+  0xFFFFFFFF00000000,
	-2147483648L	+  0xAAAAAAAA00000000
];
def main(a: int) -> int {
	return int.!(x[a]);
}