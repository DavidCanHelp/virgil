//@execute (11,0)=11; (0,1)=0; (1,100)=0; (-200,33)=0; (1, 32)=0; (-200,-10)=0
def main(a: int, b: int) -> int {
	if (b == 0) return a << 0;
	if (b == 1) return a << 1;
	if (b == 100) return a << 100;
	if (b == 33) return a << 33;
	if (b == 32) return a << 32;
	if (b == -10) return a << byte.!(-10);
	return a << byte.!(b);
}

