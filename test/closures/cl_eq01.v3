//@execute 0=true; 1=true; 2=false; 3=false; 4=false; 5=true; 6=false 
component cl_eq01 {
	def main(a: int) -> bool {
		if (a == 0) return add(_, 1) == add(_, 1);
		if (a == 1) return add(_, 2) == add(_, 2);
		if (a == 2) return add(_, 1) == add(_, 3);
		if (a == 3) return add(1, _) == add(_, 1);
		if (a == 4) return add(2, _) == add(1, _);
		if (a == 5) return add(1, _) == add(1, _);
		return false;
	}
	def add(a: int, b: int) -> int { return a + b; }
}
