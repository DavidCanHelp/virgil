//@execute 0=true; 1=true; 2=true; 3=true; 4=true; 5=true; 6=false
component cl_eqx02 {
	def main(a: int) -> bool {
		match(a) {
			0: return test(byte.==);
			1: return test(byte.!=);
			2: return test(byte.<);
			3: return test(byte.<=);
			4: return test(byte.>);
			5: return test(byte.>=);
		}
		return false;
	}
	def test<T>(f: (byte, byte) -> T) -> bool {
		var a = f('1', _);
		var b = f('2', _);
		var c = f(_, '1');
		var d = f(_, '2');
		return a == a && a != b && a != c && a != d
			&& b == b && b != c && b != d
			&& c == c && c != d
			&& d == d;
	}
}
