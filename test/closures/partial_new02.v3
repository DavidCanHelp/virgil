//@execute (1,2,3)=17; (4,5,9)=56; (-3,2,-2)=-26
class partial_new02a {
	var r: int;
	new(a: (int, int, int)) { r = 2 * a.0 - 3 * a.1 + 7 * a.2; }
}
component partial_new02 {
	var expect: int;
	var failed: int;
	def main(a: int, b: int, c: int) -> int {
		failed = 0;
		expect = partial_new02a.new(a, b, c).r;

		check(partial_new02a.new(a, _, _), (b, c));
		check(partial_new02a.new(_, b, _), (a, c));
		check(partial_new02a.new(_, _, c), (a, b));

		test2(partial_new02a.new(a, _, _), b, c);
		test2(partial_new02a.new(_, b, _), a, c);
		test2(partial_new02a.new(_, _, c), a, b);

		return if(failed == 0, expect);
	}
	def test2(f: (int, int) -> partial_new02a, p1: int, p2: int) {
		check(f(p1, _), p2);
		check(f(_, p2), p1);
	}
	def check<P>(f: P -> partial_new02a, p: P) {
		if (expect != f(p).r) failed++;
	}
}
