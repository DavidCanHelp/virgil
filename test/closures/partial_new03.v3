//@execute (1,2,3)=17; (4,5,9)=56; (-3,2,-2)=-26
class partial_new03a {
	var r: int;
	new(a: int, b: int, c: int) { r = 2 * a - 3 * b + 7 * c; }
}
component partial_new03 {
	var expect: int;
	var failed: int;
	def main(a: int, b: int, c: int) -> int {
		failed = 0;
		expect = partial_new03a.new(a, b, c).r;

		check(partial_new03a.new(a, _, _), (b, c));
		check(partial_new03a.new(_, b, _), (a, c));
		check(partial_new03a.new(_, _, c), (a, b));

		test2(partial_new03a.new(a, _, _), b, c);
		test2(partial_new03a.new(_, b, _), a, c);
		test2(partial_new03a.new(_, _, c), a, b);

		return if(failed == 0, expect);
	}
	def test2(f: (int, int) -> partial_new03a, p1: int, p2: int) {
		check(f(p1, _), p2);
		check(f(_, p2), p1);
	}
	def check<P>(f: P -> partial_new03a, p: P) {
		if (expect != f(p).r) failed++;
	}
}
