//@execute 0=true; 1=true; 2=false; 3=false; 4=false; 5=true; 6=false 
component cl_eq02 {
	def main(a: int) -> bool {
		if (a == 0) return int.+(_, 1) == int.+(_, 1);
		if (a == 1) return int.+(_, 2) == int.+(_, 2);
		if (a == 2) return int.+(_, 1) == int.+(_, 3);
		if (a == 3) return int.+(1, _) == int.+(_, 1);
		if (a == 4) return int.+(2, _) == int.+(1, _);
		if (a == 5) return int.+(1, _) == int.+(1, _);
		return false;
	}
}
