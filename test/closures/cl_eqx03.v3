//@execute 0=true; 1=true; 2=true; 3=true; 4=false
component cl_eqx03 {
	def main(a: int) -> bool {
		match(a) {
			0: return test(bool.==);
			1: return test(bool.!=);
			2: return test(bool.&&);
			3: return test(bool.||);
		}
		return false;
	}
	def test<T>(f: (bool, bool) -> T) -> bool {
		var a = f(true, _);
		var b = f(false, _);
		var c = f(_, true);
		var d = f(_, false);
		return a == a && a != b && a != c && a != d
			&& b == b && b != c && b != d
			&& c == c && c != d
			&& d == d;
	}
}
