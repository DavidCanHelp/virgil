//@execute 0=1; 1=4
class partial_tup12_a {
	def f(a: int, b: (int, int)) -> (int, int) { return (a, b.1); }
}
component partial_tup12 {
	def main(arg: int) -> int {
		var o = partial_tup12_a.new();
		var f = if (arg == 0, o.f(1, _), o.f(1, _)), x = (3, 4), t = f(x);
		if (arg == 0) return t.0;
		else return t.1;
	}
}
