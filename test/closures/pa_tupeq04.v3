//@execute (0,0)=true; (-1,0)=false; (0, -1)=false; (-9,-9)=true; (11,11)=true; (11,-11)=false
component pa_tupeq04 {
	def main(a: int, b: int) -> bool {
		var f = eq<(int, bool)>();
		var g = if(a > 0, f((a, true), _), f(_, (a, true)));
		return g(b, _)(true);
	}
	def eq<T>() -> (T, T) -> bool {
		return T.==;
	}
}
