//@execute 0=true; 1=true; 2=true; 3=true; 4=true; 5=true; 6=true; 7=true; 8=true; 9=true; 10=true; 11=true; 12=true; 13=true; 14=true; 15=true; 16=false
component cl_appx01 {
	def main(a: int) -> bool {
		match(a) {
			0: return test(int.==);
			1: return test(int.!=);
			2: return test(int.+);
			3: return test(int.-);
			4: return test(int./);
			5: return test(int.*);
			6: return test(int.%);
			7: return test(int.&);
			8: return test(int.|);
			9: return test(int.^);
			10: return test(int.#<<);
			11: return test(int.#>>);
			12: return test(int.<);
			13: return test(int.<=);
			14: return test(int.>);
			15: return test(int.>=);
		}
		return false;
	}
	def test<T>(f: (int, int) -> T) -> bool {
		var a = f(1, _);
		var b = f(2, _);
		var c = f(_, 1);
		var d = f(_, 2);
		for (i = 1; i < 5; i++) {
			if (a(i) != a(i)) return false;
			if (b(i) != b(i)) return false;
			if (c(i) != c(i)) return false;
			if (d(i) != d(i)) return false;
		}
		return true;
	}
}
