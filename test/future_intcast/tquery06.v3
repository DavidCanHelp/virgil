//@execute 0=false; 1=true; 2=true; 3=false; 4=false; 5=false; 6=false

def main(arg: int) -> bool {
	var v: void;
	if (arg == 1) return int.?<int>(0);
	if (arg == 2) return int.?<byte>('0');
	if (arg == 3) return int.?<bool>(true);
	if (arg == 4) return int.?<string>("false");
	if (arg == 5) return int.?<void>(v);
	return false;
}

