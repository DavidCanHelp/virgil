//@execute 0=true; 1=true; 2=true; 3=true; 4=false
var xs: Array<i33> = [
	i33.!(0), i33.!(1), i33.!(-1), i33.!(200), i33.!(-200), i33.!(i33.max-1), i33.!(i33.min+1), i33.!(i33.max), i33.min
];
var xu: Array<u33> = [
	u33.!(0), u33.!(1), u33.!(200), u33.!(u33.max - 200u), u33.!(u33.max - 1u), u33.!(u33.max)
];

def do_ss() -> bool {
	for (v in xs) {
		if (!i33.query(v)) return false;
	}
	return true;
}
def do_su() -> bool {
	for (v in xs) {
		var exp = v >= 0 && v <= u33.max;
		var got = u33.query(v);
		if (exp != got) return false;
	}
	return true;
}
def do_us() -> bool {
	for (v in xu) {
		var exp = v <= i33.max;
		var got = i33.query(v);
		if (exp != got) return false;
	}
	return true;
}
def do_uu() -> bool {
	for (v in xu) {
		if (!u33.query(v)) return false;
	}
	return true;
}
def main(a: int) -> bool {
	if (a == 0) return do_ss();
	if (a == 1) return do_su();
	if (a == 2) return do_us();
	if (a == 3) return do_uu();
	return false;
}