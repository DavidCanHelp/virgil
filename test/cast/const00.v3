//@execute 0=0; 1=!TypeCheckException; 2=250; 3=!TypeCheckException; 4=13; 5=128; 6=!TypeCheckException; 7=!TypeCheckException
def main(x: int) -> int {
	if (x == 0) return byte.!(0);
	if (x == 1) return byte.!(-1);
	if (x == 2) return byte.!(250);
	if (x == 3) return byte.!(256);
	if (x == 4) return byte.!(13);
	if (x == 5) return byte.!(128);
	if (x == 6) return byte.!(-127);
	return byte.!(511);
}

