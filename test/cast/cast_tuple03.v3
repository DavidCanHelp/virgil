//@execute 0=42; 1=11; 2=32; 3=33; 4=42

component cast_tuple03 {
	def main(arg: int) -> int {
		var t = (arg + 10, 32, 33);
		var f = cast<(int, int, int)>();
		if (arg == 1) return f(t).0;
		if (arg == 2) return f(t).1;
		if (arg == 3) return f(t).2;
		return 42;
	}
	def cast<A>() -> A -> A { return A.!; }
}
