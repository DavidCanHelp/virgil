//@execute 0=false; 1=true; 2=true; 3=false; 4=false; 5=false; 6=false
def main(arg: int) -> bool {
	var v: void;
	if (arg == 1) return byte.?<int>(0);
	if (arg == 2) return byte.?<byte>('0');
	if (arg == 3) return byte.?<bool>(true);
	if (arg == 4) return byte.?<string>("false");
	if (arg == 5) return byte.?<void>(v);
	return false;
}

