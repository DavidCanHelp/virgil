component cond09 {
	var result: int;
	def main(a: bool, b: bool) -> int {
		result = 0;
		record(a || a);
		record(a || b);
		record(a || true);
		record(a || false);
		record(b || a);
		record(b || b);
		record(b || true);
		record(b || false);
		record(true || a);
		record(true || b);
		record(true || true);
		record(true || false);
		record(false || a);
		record(false || b);
		record(false || true);
		record(false || false);
		return result;
	}
	def record(a: bool) {
		if (a) result = result << 1 | 1;
		else result = result << 1;
	}
}
