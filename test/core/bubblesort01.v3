//@execute 0 = -1
component bubblesort01 {
	var array: Array<int> = [0, -400000, 1, 8, 9, 1024, 0, -1, 55, -8, 7, 122, 7637782, -89237];

	def main(arg: int) -> int {
		var i = 0;
		while (i < array.length) {
			var j = i + 1;
			while (j < array.length) {
				if (array[i] > array[j]) {
					var t = array[i];
					array[i] = array[j];
					array[j] = t;
				}
				j = j + 1;
			}
			i = i + 1;
		}
		return array[3];
	}
}
