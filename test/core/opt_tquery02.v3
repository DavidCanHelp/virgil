//@execute 0=7; 1=3; 2=6; 3=5; 4=6
class A { }
class X extends A { }
class Y extends A { }
class Z extends A { }

def array = [A.new(), X.new(), null, Z.new(), null];

def main(i: int) -> int {
	match (array[i]) {
		x: X => return 3;
		x: Y => return 4;
		x: Z => return 5;
		x: A => return 7;
	}
	return 6;
}
