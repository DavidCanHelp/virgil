//@execute 0=15; 1=0; 2=0; 3=0; 4=2
def main(a: int) -> int {
	match (a) {
		0 => return do<int, int>();
		1 => return do<int, void>();
		2 => return do<int, byte>();
		3 => return do<int, bool>();
	}
	return 2;
}
def do<A, B>() -> int {
	var a = q<A -> int, B -> int>();
	var b = q<B -> int, A -> int>();
	var c = q<int -> A, int -> B>();
	var d = q<int -> B, int -> A>();
	return (a << 3) | (b << 2) | (c << 1) | d;
}
def q<A, B>() -> int {
	var none: A;
	return if (B.?(none), 1, 0);
}
