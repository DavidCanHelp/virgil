//@execute (1,2,3,4,5)=3; (2,4,5,3,1)=3; (2,2,4,3,5)=3; (1,3,3,3,5)=3
def main(var a: int, var b: int, var c: int, var d: int, var e: int) -> int {
	var median: int, t1: int, t2: int;

	// sort a and b
	if (a > b) {
		t1 = a;
		a = b;
		b = t1;
	}

	// sort d and e
	if (d > e) {
		t1 = d;
		d = e;
		e = t1;
	}

	// swap the pairs (a, b) and (d, e) if necessary
	if (d < a) {
		t1 = d;
		t2 = e;
		d = a;
		e = b;
		a = t1;
		b = t2;
	}

	if (b < c) {
		if (b < d) {
			if (c < d) return c;
			else return d;
		} else {
			return b;
		}
	} else {
		if (c < d) {
			if (b < d) return b;
			else return d;
		} else {
			if (c < e) return c;
			else return e;
		}
	}
}
