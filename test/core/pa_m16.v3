//@execute false=3; true=12
class pa_m16 {
	def m<T>() -> Array<T> { return Array<T>.new(3); }
}
def main(a: bool) -> int {
	var o = pa_m16.new();
	var f = o.m<int>, g = o.m<byte>;
	return if(a, est(f), est(g));
}
def est<T>(f: () -> Array<T>) -> int {
	var a = f();
	if (Array<byte>.?(a)) return 1 * a.length;
	else return 4 * a.length;
}
