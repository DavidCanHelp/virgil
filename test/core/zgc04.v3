//@execute = 0
class zgc04(a: int) {
	def array = [a];
	var other = [array.length, array[0]];
	def validate() {
		if (array.length != 1) errors++;
		if (array[0] != a) errors++;
		if (other.length != 2) errors++;
		if (other[0] != 1) errors++;
		if (other[1] != a) errors++;
	}
	def run() {
		validate();
		other = [array.length, array[0]];
		validate();
	}
}
var errors: int;
def main() -> int {
	errors = 0;
	run(5);
	return errors;
}
def run(count: int) {
	var f = m();
	for (i = 0; i < count; i++) f();
}
def m() -> void -> void {
	return zgc04.new(11).run;
}
