component comp17 {
	var result: int;
	def main(a: int, b: int) -> int {
		result = 0;
		add(a == b);
		add(b == a);
		add(a < b);
		add(b < a);
		add(a <= b);
		add(b <= a);
		add(a >= b);
		add(b >= a);

		add(a == 0);
		add(b == 0);
		add(a < 0);
		add(b < 0);
		add(a <= 0);
		add(b <= 0);
		add(a >= 0);
		add(b >= 0);

		add(0 == b);
		add(0 == a);
		add(0 < b);
		add(0 < a);
		add(0 <= b);
		add(0 <= a);
		add(0 >= b);
		add(0 >= a);

		add(a == -1);
		add(b == -1);
		add(a < -1);
		add(b < -1);
		add(a <= -1);
		add(b <= -1);
		add(a >= -1);
		add(b >= -1);
		return result;
	}
	def add(b: bool) {
		if (b) result = (result << 1) | 1;
		else result = result << 1;
	}
}
