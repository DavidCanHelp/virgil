//@execute 0=1; 1=0; 2=33; 3=11
def q<T>(v: T) -> int {
	if (int.?(v)) return int.!(v);
	if (bool.?(v)) return if(bool.!(v), 1, 0);
	return 0;
}
def r<T>(v: T, d: int) -> int {
	if (d > 0) return r(v, d - 1);
	return q(v);
}
def main(a: int) -> int {
	if (a == 0) return r(true, 2);
	if (a == 1) return r(false, 3);
	if (a == 2) return r(33, 1);
	return 11;
}
