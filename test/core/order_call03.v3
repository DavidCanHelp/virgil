//@execute 0=!NullCheckException; 1=!DivideByZeroException; 2=!DivideByZeroException
class order_call03a {
	def f(a: int) -> int {
		var o: order_call03a;
		if (a > 0) o = this;
		return o.m(div0(a)) + o.m(a) + o.m(a);
	}
	private def m(a: int) -> int {
		return a;
	}
	def div0(a: int) -> int {
		return a / 0;
	}
}
component order_call03 {
	var g: order_call03a = order_call03a.new();
	def main(a: int) -> int {
		return g.f(a);
	}
}
