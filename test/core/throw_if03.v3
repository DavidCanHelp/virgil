//@execute 0=!DivideByZeroException; 1=!DivideByZeroException; 2=!DivideByZeroException; 4=4
component throw_if03 {
	def main(a: int) -> int {
		var x = a;
		if (a == 0) if (a == a/0) x = 11; else x = 12;
		if (a == 1) if (a == 1) x = 1/0; else x = 12;
		if (a == 2) if (a == 1) x = 11; else x = 1/0;
		return x;
	}
}
