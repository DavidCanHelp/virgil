//@execute (0,0)=!DivideByZeroException; (1,5)=0; (1,-5)=0; (2,11)=1; (2,-11)=-1; (3,21)=1; (3,-21)=-1; (4,41)=1; (4,-41)=-1; (5,5)=5
component opt_mod02 {
	def main(a: int, b: int) -> int {
		if (a == 0) return b % 0;
		if (a == 1) return b % 1;
		if (a == 2) return b % 2;
		if (a == 3) return b % 4;
		if (a == 4) return b % 8;
		return b;
	}
}
