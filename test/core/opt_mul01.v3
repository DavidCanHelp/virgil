//@execute (0,1)=0; (0,-1)=0; (1,5)=5; (1,-5)=-5; (2,5)=10; (2,-5)=-10; (3,5)=20; (3,-5)=-20; (4,5)=40; (4,-5)=-40; (5,5)=5
component opt_mul01 {
	def main(a: int, b: int) -> int {
		if (a == 0) return b * 0;
		if (a == 1) return b * 1;
		if (a == 2) return b * 2;
		if (a == 3) return b * 4;
		if (a == 4) return b * 8;
		return b;
	}
}
