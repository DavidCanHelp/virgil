//@execute 5=80; 6=96; 10=160
component for16 {
	def main(a: int) -> int {
		var x = 0;
		for (i = 0; true; i = i + 1) {
			if (i == 2) break;
			for (j = 0; true; j = j + 1) {
				if (j == 2) break;
				for (k = 0; true; k) {
					if (k == 2) break;
					var l = 0;
					while (true) {
						if (l == 2) break;
						var m = 0;
						while (true) {
							if (m == a) break;
							x = x + 1;
							m = m + 1;
						}
						l = l + 1;
					}
					k = k + 1;
				}
			}
		}
		return x;
	}
}
