//@execute 0=1; 1=1; 2=1; 3=1; 4=2
def main(a: int) -> int {
	match (a) {
		0 => return do<int, int>();
		1 => return do<byte, byte>();
		2 => return do<bool, bool>();
		3 => return do<void, void>();
	}
	return 2;
}
def do<A, B>() -> int {
	var none: (A, int);
	return if(q<(A, int), (B, int)>()(none), 1, 0);
}
def q<A, B>() -> A -> bool {
	return B.?<A>;
}
