//@execute 0=10; 1=11; 2=22; 255=300; 5=42
component match_b02 {
	def b0: byte = '\x00';
	def b1: byte = '\x01';
	def b2: byte = '\x02';
	def bFF: byte = '\xFF';
	def main(a: int) -> int {
		match (a) {
			b0 => return 10;
			b1 => return 11;
			b2 => return 22;
			bFF => return 300;
		}
		return 42;
	}
}
