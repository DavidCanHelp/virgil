class fib {

  var MAX: int = 200;
  var title: string = "Fibonacci program - VIRGIL\n";

  def main() {
    var x1 = 1, x2 = 2, cntr: int = 0;

    print(title);

    while (cntr < MAX) {
       var next = x1 + x2;
       print("The next Fibonacci number is: "+next+"\n");
       x1 = x2;
       x2 = next;

	cntr = cntr + 1;
    }

    return 0;
  }
}
