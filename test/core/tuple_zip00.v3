//@execute 0=1
component tuple_zip00 {
	def main(arg: int) -> int {
		var r = zip([2], [-1]);
		return sum(r);
	}
	def zip<A, B>(a: Array<A>, b: Array<B>) -> Array<(A, B)> {
		var r = Array<(A, B)>.new(a.length), i = 0;
		while (i < r.length) {
			r[i] = (a[i], b[i]);
			i = i + 1;
		}
		return r;
	}
	def sum(a: Array<(int, int)>) -> int {
		var i = 0, sum = 0;
		while (i < a.length) {
			sum = sum + a[i].0 + a[i].1;
			i = i + 1;
		}
		return sum;
	}
}
