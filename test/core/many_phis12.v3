component many_phis12 {
	def main(a: int) -> int {
		var x0 = z(), x1 = z(), x2 = z(), x3 = z(), x4 = z(), x5 = z(), x6 = z(), x7 = z(), x8 = z(), x9 = z();
		match (a % 10) {
			0 => x0 = 1 - x0;
			1 => x1 = 2 - x1;
			2 => x2 = 3 - x2;
			3 => x3 = 4 - x3;
			4 => x4 = 5 - x4;
			5 => x5 = 6 - x5;
			6 => x6 = 7 - x6;
			7 => x7 = 8 - x7;
			8 => x8 = 9 - x8;
			9 => x9 = 10 - x9;
		}
		return a + x0 + x1 + x2 + x3 + x4 + x5 + x6 + x7 + x8 + x9;
	}
	def z() -> int { return 0; }
}
