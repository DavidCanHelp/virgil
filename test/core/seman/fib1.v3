class fib {
    var MAX: int = 200;
    var title: string = "Fibonacci program - VIRGIL\n";
    
    def main() -> int {
        var x1: int = 1,
              x2: int = 2;
        var cntr: int;

	cntr = 0;
        while (cntr < MAX) {
            var next: int = x1 + x2;

            x1 = x2;
            x2 = next;

            if (cntr == MAX - 1) break;
            else return ();
	    cntr = cntr + 1;
        }
        return 0;
    }
}
