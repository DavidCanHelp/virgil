//@execute 0=true; 1=true; 2=true; 3=true; 4=false
def cmp(a: int, b: int) -> bool {
	return a == b;
}
def main(a: int) -> bool {
	match(a) {
		0 => return cmp(int.min, -2147483648);
		1 => return cmp(-2147483648, -2147483648);
		2 => return cmp(-2147483648, int.min);
		3 => return cmp(int.min, int.min);
	}
	return false;
}
