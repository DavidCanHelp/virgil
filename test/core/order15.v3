//@execute 0=0; 1=2; 2=3; 3=0; 4=12; 5=12; 6=0

component order15 {
    var order: Array<int> = [0, 0, 0];
    var pos: int = 0;

    var data: Array<int> = [7];

    def first() -> Array<int> {
        order[pos] = 1;
        pos = pos + 1;
        return data;
    }

    def second(a: int) -> int {
        order[pos] = 2;
        pos = pos + 1;
        return a;
    }

    def third(a: int) -> int {
        order[pos] = 3;
        pos = pos + 1;
        return a;
    }

    def main(arg: int) -> int {
	pos = 0;
        var result = data[second(0)] = third(12);
	if (arg == 1) return order[0];
	if (arg == 2) return order[1];
	if (arg == 3) return order[2];
	if (arg == 4) return result;
	if (arg == 5) return data[0];
	return 0;
    }
}
