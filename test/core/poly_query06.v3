//@execute 0=1; 1=0; 2=33; 3=11
class poly_query06 {
	def q<T>(v: T) -> int {
		if (int.?(v)) return int.!(v);
		if (bool.?(v)) return if(bool.!(v), 1, 0);
		return 0;
	}
	def r<T>(v: T) -> int {
		return q(v);
	}
}
def main(a: int) -> int {
	var o = poly_query06.new();
	if (a == 0) return o.r(true);
	if (a == 1) return o.r(false);
	if (a == 2) return o.r(33);
	return 11;
}
