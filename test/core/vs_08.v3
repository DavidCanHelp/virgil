class vs_08a<T> {
	def m() -> int { return 1; }
	def n() -> int { return Array<T>.new(2).length; }
	def o() -> int { return 3; }
}
class vs_08b<T> extends vs_08a<T> {
}
def main(a: int) -> int {
	return if(a == 0, x(0) + x(1), y(0) + y(1));
}
def x(i: int) -> int {
	var o = if(i == 0, vs_08a<byte>.new(), vs_08b<byte>.new());
	return o.m() + o.n() + o.o();
}
def y(i: int) -> int {
	var o = if(i == 0, vs_08a<bool>.new(), vs_08b<bool>.new());
	return o.n();
}
