//@execute false=!DivideByZeroException; true=!DivideByZeroException
def throw() -> int {
	return 1 / 0;
}
def f(a: bool) -> int {
	return if(a, throw(), 0);
}
def g(a: bool) -> int {
	return if(a, 0, throw());
}
def funcs = [f, g];
def main(a: bool) -> int {
	var sum = 0;
	for (f in funcs) sum += f(a);
	return sum;
}
