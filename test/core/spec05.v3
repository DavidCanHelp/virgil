//@execute 0=true; 1=true; 2=true; 3=true; 4=false
def none<T>(a: T) {
	if (int.?(a)) return;
}
def getNone<T>() -> T -> void {
	var x = Array<T>.new(3);
	return none<T>;
}
def main(a: int) -> bool {
	match (a) {
		0 => return none<void> == getNone<void>();
		1 => return none<byte> == getNone<byte>();
		2 => return none<bool> == getNone<bool>();
		3 => return none<int> == getNone<int>();
	}
	return false;
}
