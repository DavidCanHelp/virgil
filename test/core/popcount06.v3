//@execute 0=0; -1=32; 8=1; 67=3; 127=7; 6=2; 4=1; 11=3; 82734222=15; -82734222=17
def main(var x: int) -> int {
	// fast def for popcount from the Aggregate Magic Algorithm
	x = x - ((x >>> 1) & 0x55555555);
	x = ((x >>> 2) & 0x33333333) + (x & 0x33333333);
	x = ((x >>> 4) + x) & 0x0f0f0f0f;
	x = x + (x >>> 8);
	x = x + (x >>> 16);
	return x & 0x3f;
}
