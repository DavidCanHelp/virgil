component ptr_endian02 {
	def b: Array<int> = [0];
	def main(a: int) -> int {
		var p = Pointer.atContents(b);
		p.store<int>(a);
		swap<byte>(p.add(0), p.add(3));
		swap<byte>(p.add(1), p.add(2));
		return p.load<int>();
	}
	def swap<T>(pa: Pointer, pb: Pointer) {
		var v = pa.load<T>();
		pa.store(pb.load<T>());
		pb.store(v);
	}
}
