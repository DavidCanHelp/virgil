//@execute 0=false; 1=true; 2=true; 3=true; 4=false; 5=false
component ptr_wrap02 {
	def main(a: int) -> bool {
		var n: Pointer, m = n.add(-1);
		var cmp = Pointer.==;
		match (a) {
			1: cmp = Pointer.!=;
			2: cmp = Pointer.lt;
			3: cmp = Pointer.lteq;
			4: cmp = Pointer.gt;
			5: cmp = Pointer.gteq;
		}
		return cmp(n, m);
	}
}
