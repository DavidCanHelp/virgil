//@execute = true
def SYS_OPEN = 5;
def SYS_READ = 3;
def SYS_LSEEK = 199;
var buffer: Array<byte> = Array.new(35);
var expect: Array<byte> = "The quick brown fox jumped over the lazy dog.";
def main() -> bool {
	var fd = Darwin.syscall(SYS_OPEN, (Pointer.atContents("test.txt\x00"), 0, 0)).0;
	if (Darwin.syscall(SYS_LSEEK, (fd, (10, 0), 0)).0 != 10) return false;
	if (Darwin.syscall(SYS_READ, (fd, Pointer.atContents(buffer), 35)).0 != 35) return false;
	for (i = 0; i < buffer.length; i++) {
		if (buffer(i) != expect(i + 10)) return false;
	}
	return true;
}
