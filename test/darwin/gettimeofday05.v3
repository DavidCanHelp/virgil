//@execute = true
component gettimeofday05 {
	def timeval: Array<int> = Array.new(2);
	def timezone: Array<int> = Array.new(2);
	def main() -> bool {
		var tv = Pointer.atContents(timeval), tz = Pointer.atContents(timezone);
		var before = Darwin.gettimeofday(tv, tz).1;
		for (i = 0; i < 100; i++) {
			var after = Darwin.gettimeofday(tv, tz).1;
			if (before != after) return true;
		}
		return false;
	}
}
