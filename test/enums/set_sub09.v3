//@execute 0=3; 1=3; 2=1; 3=1; 4=2; 5=2; 6=0; 7=0
enum C { Q, R, S }
def none: C.set;
def sets: Array<C.set> = [
	C.Q | C.R | C.S,
	C.Q | C.R      ,
	C.Q |       C.S,
	C.Q            ,
	      C.R | C.S,
	      C.R      ,
	            C.S,
	none
];
def main(a: int) -> int {
	return bits(sets[a] - C.S);
}
def bits(set: C.set) -> int {
	var x = 0;
	for (b in set) x |= 1 << b.tag;
	return x;
}
