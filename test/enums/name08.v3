//@execute 0 = true; 1 = true; 2 = false
enum name03 { ADD, SUB }
def eq(x: string, y: string) -> bool {
	for (i = 0; i < x.length; i++) if (x[i] != y[i]) return false;
	return true;
}
def main(a: int) -> bool {
	if (a == 0) return eq(name03.ADD.name, "ADD");
	if (a == 1) return eq(name03.SUB.name, "SUB");
	return false;
}
