//@execute = 112
enum E { A, B, C, D }
def MAX = E.A | E.B | E.C | E.D;
def main() -> int {
	for (x < 16) for (y < 16) test(get(x), get(y));
	return 112;
}
def Eint = E.set.&;
def test(x: E.set, y: E.set) -> int {
	if ((x - y) != Eint(x, (MAX - y))) return 1/0;
	return 1;
}
def get(x: int) -> E.set {
	var set: E.set;
	for (e in E.set.all) if ((x & 1 << e.tag) != 0) set |= e;
	return set;
}