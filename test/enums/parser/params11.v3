//@parse
enum Infix(op: string, prec: int, flags: InfixFlags.set) {
	Equal	("=="),
	NotEqual("!="),
	Add	("+"),
	Sub	("-"),
	Mul	("*"),
	Div	("/"),
	Mod	("%"),
	And	("&"),
	Or	("|"),
	AndAnd	("&&"),
	OrOr	("||"),
	Xor	("^"),
	Shl	("#<<"),
	Shr	("#>>"),
	Lt	("<"),
	Gt	(">"),
	Lteq	("<="),
	Gteq	(">="),
	QShl	("<<"),
	QSar	(">>"),
	QShr	(">>>")
}
