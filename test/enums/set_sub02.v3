//@execute 0=7; 1=6; 2=5; 3=4; 4=3; 5=2; 6=1; 7=0; 8=!BoundsCheckException
enum T { U, V, W }
def none: T.set;
def sets: Array<T.set> = [
	T.U | T.V | T.W,
	T.U | T.V      ,
	T.U |       T.W,
	T.U            ,
	      T.V | T.W,
	      T.V      ,
	            T.W,
	none
];
def main(a: int) -> int {
	return bits(sets[a]);
}
def bits(a: T.set) -> int {
	var x = 0;
	if (a.W) x |= 1;
	if (a.V) x |= 2;
	if (a.U) x |= 4;
	return x;
}
