//@execute 0=1; 1=1; 2=1; 3=11
enum E {
	A0, A1, A2, A3, A4, A5, A6, A7, A8, A9, Aa, Ab, Ac, Ad, Ae, Af,
	C0, C1, C2, C3, C4, C5, C6, C7, C8, C9, Ca, Cb, Cc, Cd, Ce, Cf,
	B0, B1, B2, B3, B4, B5, B6, B7, B8, B9, Ba, Bb, Bc, Bd, Be, Bf
}
def none: E.set;
def SM: E.set = E.A0;
def SN: E.set = E.B1;
def SO: E.set = E.Bf;
def main(a: int) -> int {
	if (a == 0) return check([
		E.Bf  | E.B1,
		none | SN   | SO,
		SN   | none | SO,
		none | SN   | SO
	]);
	if (a == 1) return check([
		E.B1  | E.A0,
		none | SN   | SM,
		SN   | SM   | none,
		SN   | none | SM
	]);
	if (a == 2) return check([
		E.A0  | E.Bf,
		none | SM   | SO,
		SM   | none | SO,
		SM   | SO   | none
	]);
	return 11;
}
def check(a: Array<E.set>) -> int {
	for (x in a) for (y in a) {
		if (x != y) return 1 / 0;
	}
	return 1;
}