//@execute 0=true; 1=true; 2=true; 3=true; 4=false
var xs: Array<i63> = [
	i63.!(0), i63.!(1), i63.!(-1), i63.!(200), i63.!(-200), i63.!(i63.max-1), i63.!(i63.min+1), i63.!(i63.max), i63.min
];
var xu: Array<u63> = [
	u63.!(0), u63.!(1), u63.!(200), u63.!(u63.max - 200u), u63.!(u63.max - 1u), u63.!(u63.max)
];

def do_ss() -> bool {
	for (v in xs) {
		if (!i63.query(v)) return false;
	}
	return true;
}
def do_su() -> bool {
	for (v in xs) {
		var exp = v >= 0 && v <= u63.max;
		var got = u63.query(v);
		if (exp != got) return false;
	}
	return true;
}
def do_us() -> bool {
	for (v in xu) {
		var exp = v <= i63.max;
		var got = i63.query(v);
		if (exp != got) return false;
	}
	return true;
}
def do_uu() -> bool {
	for (v in xu) {
		if (!u63.query(v)) return false;
	}
	return true;
}
def main(a: int) -> bool {
	if (a == 0) return do_ss();
	if (a == 1) return do_su();
	if (a == 2) return do_us();
	if (a == 3) return do_uu();
	return false;
}