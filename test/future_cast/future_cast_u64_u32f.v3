def do0() -> u32 { return u32.cast(0x0000000000000000u); }
def do1() -> u32 { return u32.cast(0x0000000000000001u); }
def do2() -> u32 { return u32.cast(0x0000000010000000u); }
def do3() -> u32 { return u32.cast(0x0000000020000000u); }
def do4() -> u32 { return u32.cast(0x0000000040000000u); }
