def do8() -> u32 { return u32.cast(0xffffffff80000000u); }
def do9() -> u32 { return u32.cast(0xffffffff00000000u); }
def y: Array<u32> = [
    0x00000000u,
    0x00000001u,
    0x10000000u,
    0x20000000u,
    0x40000000u,
    0x50000000u,
    0x80000000u,
