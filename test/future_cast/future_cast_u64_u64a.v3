def x: Array<u64> = [
    0x0000000000000000u,
    0x0000000100000000u,
    0x2000000000000000u,
    0x4000000000000000u,
    0x6000000000000000u,
    0x8000000000000000u,
    0xa000000000000000u,
    0xc000000000000000u,
    0xe000000000000000u
];
def y: Array<u64> = [
    u64.view(0x0000000000000000),
    u64.view(0x0000000100000000),
    u64.view(0x2000000000000000),
    u64.view(0x4000000000000000),
    u64.view(0x6000000000000000),
    u64.view(0x8000000000000000),
    u64.view(0xa000000000000000),
    u64.view(0xc000000000000000),
