def main(args: Array<string>) {
	def v = Vector<byte>.new();
	def buf = StringBuilder.new();
	for (a in args) {
		v.resize(0);
		for (i = 0; i < a.length; i += 2) {
			var b1 = Chars.hexValue(a[i]);
			var b0 = Chars.hexValue(a[i+1]);
			v.put(byte.view((b1 << 4) | b0));
		}

		if (v.length == 0) continue;

		for (i < v.length) buf.putx_8(v[i]);
		buf.sp();
		var f = v[0];
		if ((f & 0xF0) == 0x40) {
			buf.put1("W=%c ", bit(f, 3));
			buf.put1("R=%c ", bit(f, 2));
			buf.put1("I=%c ", bit(f, 1));
			buf.put1("B=%c ", bit(f, 0));
			buf.ln();
			System.fileWriteK(1, buf.buf, 0, buf.length);
		}
	}
}
def bit(b: byte, index: u5) -> byte {
	return if ((b & (1 << index)) != 0, '1', ' ');
}