var x = 0;
var y = 0;
def gx(v: bool) -> bool {
	x += 1;
	return v;
}
def gy(v: bool) -> bool {
	y += 10;
	return v;
}
def main(a: bool, b: bool) -> int {
	x = 0;
	y = 0;
	if (gx(a) | gy(b)) return x + y + 100;
	return x + y + 200;
}
