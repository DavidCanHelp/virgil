def x = [0];
def throw() -> int {
	return x(1);
}
def loop0() -> int {
	var i = 0;
	while (throw() < 0) i++;
	return i;
}
def loop1() -> int {
	var i = 0;
	while (i < 3) i = throw();
	return i;
}
def loop2() -> int {
	var i = 0;
	for (i = throw(); i < 3; i++) ;
	return i;
}
def loop3() -> int {
	var i = 0;
	for (i = 0; i < throw(); i++) ;
	return i;
}
def loop4() -> int {
	var i = 0;
	for (i = 0; i < 3; i = throw()) ;
	return i;
}
def loop5() -> int {
	var i = 0;
	for (i = 0; i < 3; i++) throw();
	return i;
}
def main(a: int) -> int {
	return [loop0, loop1, loop2, loop3, loop4, loop5](a)();
