//@execute 0=11; 3=14; 11115=11126
def main(a: int) -> int {
	var c = Counter.new(a);
	c.do();
	if (c.a != 1) return -112;
	if (c.b != 2) return -113;
	if (c.c != 3) return -114;
	return c.storage(0);
}
class Counter(start: int) {
	var order = 1;
	var a = -1;
	var b = -1;
	var c = -1;
	def storage = [start];
	def geta() -> Array<int> { a = order++; return storage; }
	def getb() -> int { b = order++; return 0; }
	def getc() -> int { c = order++; return 11; }
	def do() {
		geta()(getb()) += getc();
	}
}
