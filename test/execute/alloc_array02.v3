//@execute 0=false; 1=true; 2=false; 3=false; 4=true; 5=false

component alloc_array02 {
    var a: Array<bool> = array(true, false);
    var b: Array<bool> = array(false, true);

    def array(c: bool, d: bool) -> Array<bool> {
       var arr = [c, d];
       return arr;
    }

    def main(arg: int) -> bool {
	if (arg == 1) return a(0);
	if (arg == 2) return a(1);
	if (arg == 3) return b(0);
	if (arg == 4) return b(1);
	return false;
    }
}
