//@execute 0=2
class jopt3a {
	def input: Array<byte>;
	var curPos: int;
	var curByte: byte;
	new(input) { curByte = input(0); }
}
component jopt3 {
	def main(a: int) -> int {
		if (a == 0) return optKeyword(jopt3a.new("if ("), "if");
		return 42;
	}
	def optKeyword(p: jopt3a, kw: string) -> int {
		var i = p.curPos, d = p.input, max = i + kw.length;
		for (c in kw) {
			if (d(i++) != c) return p.curPos;
		}
		return max;
	}
}
