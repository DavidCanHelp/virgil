//@execute 0=1; 1=49; 2=3; 3=31; 4=11
class poly_c01<T> {
	def t: T;
	new(t) { }
}
def main(a: int) -> int {
	if (a == 0) return poly_c01.new(1).t;
	if (a == 1) return poly_c01.new('1').t;
	if (a == 2) return poly_c01.new("abc").t.length;
	if (a == 3) return poly_c01.new(("abc", 31)).t.1;
	return 11;
}
