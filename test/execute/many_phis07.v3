//@execute 0=3; 1=12; 2=102; 3=12; 4=21; 5=111; 6=102; 7=111; 8=201; 9=12; 10=21; 11=111; 12=21; 13=30; 14=120; 15=111; 16=120; 17=210; 18=102; 19=111; 20=201; 21=111; 22=120; 23=210; 24=201; 25=210; 26=300
def main(i: int) -> int {
	var x: int, y: int, z: int;
	var a = 1 + (i >> 5), b = 10 + (i >> 5), c = 100 + (i >> 5);
	match(i) {
		0 =>  { x = a; y = a; z = a; }
		1 =>  { x = a; y = a; z = b; }
		2 =>  { x = a; y = a; z = c; }
		3 =>  { x = a; y = b; z = a; }
		4 =>  { x = a; y = b; z = b; }
		5 =>  { x = a; y = b; z = c; }
		6 =>  { x = a; y = c; z = a; }
		7 =>  { x = a; y = c; z = b; }
		8 =>  { x = a; y = c; z = c; }
		9 =>  { x = b; y = a; z = a; }
		10 => { x = b; y = a; z = b; }
		11 => { x = b; y = a; z = c; }
		12 => { x = b; y = b; z = a; }
		13 => { x = b; y = b; z = b; }
		14 => { x = b; y = b; z = c; }
		15 => { x = b; y = c; z = a; }
		16 => { x = b; y = c; z = b; }
		17 => { x = b; y = c; z = c; }
		18 => { x = c; y = a; z = a; }
		19 => { x = c; y = a; z = b; }
		20 => { x = c; y = a; z = c; }
		21 => { x = c; y = b; z = a; }
		22 => { x = c; y = b; z = b; }
		23 => { x = c; y = b; z = c; }
		24 => { x = c; y = c; z = a; }
		25 => { x = c; y = c; z = b; }
		26 => { x = c; y = c; z = c; }
	}
	return x + y + z;
}

