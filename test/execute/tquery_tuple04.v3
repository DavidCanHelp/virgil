//@execute 0=0; 1=0; 2=0; 3=0; 4=2
def main(a: int) -> int {
	match (a) {
		0 => return do<Array<void>, Array<int>>();
		1 => return do<Array<int>,  Array<byte>>();
		2 => return do<Array<byte>, Array<bool>>();
		3 => return do<Array<bool>, Array<void>>();
	}
	return 2;
}
def do<A, B>() -> int {
	var none: (A, int);
	return if(q<(A, int), (B, int)>()(none), 1, 0);
}
def q<A, B>() -> A -> bool {
	return B.?<A>;
}
