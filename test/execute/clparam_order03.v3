//@execute 0=33; 1=34; 2=35; 3=36; 4=42
var order = 33;
class clparam_order03a(a: int) {
	var x = order++;
}
class clparam_order03b(b: int) extends clparam_order03a(order++) {
	var y = order++;
}
def x = clparam_order03b.new(order++);
def main(a: int) -> int {
	match (a) {
		0: return x.b;
		1: return x.y;
		2: return x.a;
		3: return x.x;
	}
	return 42;
}
