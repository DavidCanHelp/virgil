//@execute 0=9; 1=9; 2=9; 3=9; 4=999
type vt_cmp18a<Y> { case E; case F; }
def app<T>(f: (vt_cmp18a<T>, vt_cmp18a<T>) -> bool) -> int {
	return
		if(f(vt_cmp18a.E, vt_cmp18a.E), 0b0001) |
		if(f(vt_cmp18a.E, vt_cmp18a.F), 0b0010) |
		if(f(vt_cmp18a.F, vt_cmp18a.E), 0b0100) |
		if(f(vt_cmp18a.F, vt_cmp18a.F), 0b1000);

}
def main(a: int) -> int {
	if (a == 0) return app(vt_cmp18a<void>.==);
	if (a == 1) return app(vt_cmp18a<byte>.==);
	if (a == 2) return app(vt_cmp18a<bool>.==);
	if (a == 3) return app(vt_cmp18a<int>.==);
	return 999;
}
