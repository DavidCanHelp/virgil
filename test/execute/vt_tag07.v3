//@execute (0,0)=true; (0,1)=false; (1,0)=false; (1,1)=true
type vt_tag07a<T> {
	case A;
	case B;
}
type vt_tag07b<U> {
	case I;
	case J;
}
def check<A, B>(fa: A -> u1, a: A, fb: B -> u1, b: B) -> bool {
	return fa(a) == fb(b);
}
def main(a: int, b: int) -> bool {
	return check<vt_tag07a<bool>, vt_tag07b<int>>
		(vt_tag07a.tag, if(a == 0, vt_tag07a.A, vt_tag07a.B),
		 vt_tag07b.tag, if(b == 0, vt_tag07b.I, vt_tag07b.J));
}
