//@execute 0=1; 1=8; 2=5; 3=20
component foreach19 {
	def main(a: int) -> int {
		var x = 1, y = 2, z = 3;
		for (i = 0; i < a; i++) {
			for (e in [0, 1, 2, 3, 4, 5, 6, 7]) {
				match (e & 3) {
					0 => x++;
					1 => { y = y + 3; continue; }
					2 => continue;
				}
				z++;
			}
		}
		return if((a & 1) == 0, x, y);
	}
}
