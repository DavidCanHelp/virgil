//@execute 0=2; 1=2; 2=2; 3=2; 4=3
def new2<T>(a: T, b: T) -> Array<T> {
	return [a, b];
}
class poly_a05a { }
class poly_a05b { }
def x = poly_a05a.new();
def y = poly_a05b.new();
def main(a: int) -> int {
	match(a) {
		0: return new2(x, null).length;
		1: return new2(y, null).length;
		2: return new2("a", "b").length;
		3: return new2(1, 0).length;
	}
	return 3;
}
