//@execute 0=true; 1=true; 2=true; 3=true; 4=true; 5=true; 6=true; 7=true; 8=true; 9=true; 10=true; 11=true; 12=true; 13=true; 14=false
component op_eq06 {
	def main(a: int) -> bool {
		match (a) {
			0: return check(bool.||);
			1: return check(bool.&&);
			2: return check(int.==);
			3: return check(int.!=);
			4: return check(void.==);
			5: return check(void.!=);
			6: return check(byte.==);
			7: return check(byte.!=);
			8: return check(byte.<);
			9: return check(byte.<=);
			10: return check(byte.>);
			11: return check(byte.>=);
			12: return check(bool.==);
			13: return check(bool.!=);
		}
		return false;
	}
	def id<T>(a: T) -> T {
		return a;
	}
	def check<T>(a: T) -> bool {
		return a == id(a);
	}
}