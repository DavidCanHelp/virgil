def main(a: int) -> bool {
	match (a) {
		0 => return check(int.+);
		1 => return check(int.-);
		2 => return check(int.*);
		3 => return check(int./);
		4 => return check(int.%);
		5 => return check(int.&);
		6 => return check(int.|);
		7 => return check(int.^);
		8 => return check(int.<<);
		9 => return check(int.>>);
		10 => return check(int.>>>);
		11 => return check(int.<);
		12 => return check(int.<=);
		13 => return check(int.>);
		14 => return check(int.>=);
	}
	return false;
}
def id<T>(a: T) -> T {
	return a;
}
def check<T>(a: T) -> bool {
	return a == id(a);
}
