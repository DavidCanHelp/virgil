//@execute 0 = true; 1 = true; 2 = false
type vt_name03 {
	case ADD;
	case SUB;
}
def eq(x: string, y: string) -> bool {
	for (i = 0; i < x.length; i++) if (x(i) != y(i)) return false;
	return true;
}
def main(a: int) -> bool {
	if (a == 0) return eq(vt_name03.ADD.name, "ADD");
	if (a == 1) return eq(vt_name03.SUB.name, "SUB");
	return false;
}
