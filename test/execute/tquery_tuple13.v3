//@execute 0=0; 1=0; 2=1; 3=0; 4=2
def main(a: int) -> int {
	match (a) {
		0: return do<Array<void>, string>([]);
		1: return do<Array<int>,  string>([]);
		2: return do<Array<byte>, string>([]);
		3: return do<Array<bool>, string>([]);
	}
	return 2;
}
def do<A, B>(v: A) -> int {
	var none = (v, 12);
	return if(q<(A, int), (B, int)>()(none), 1, 0);
}
def q<A, B>() -> A -> bool {
	return B.?<A>;
}
