component opt_commute03 {
	def main(a: int, b: byte) -> bool {
		if (a == 0) return '0' < b;
		if (a == 1) return '0' <= b;
		if (a == 2) return '0' == b;
		if (a == 3) return '0' >= b;
		if (a == 4) return '0' > b;
		return false;
	}
}
