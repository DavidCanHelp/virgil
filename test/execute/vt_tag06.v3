//@execute (0,0)=true; (0,1)=false; (1,0)=false; (1,1)=true
type vt_tag06a {
	case A;
	case B;
}
type vt_tag06b {
	case I;
	case J;
}
def check<A, B>(fa: A -> u1, a: A, fb: B -> u1, b: B) -> bool {
	return fa(a) == fb(b);
}
def main(a: int, b: int) -> bool {
	return check(vt_tag06a.tag, if(a == 0, vt_tag06a.A, vt_tag06a.B),
			vt_tag06b.tag, if(b == 0, vt_tag06b.I, vt_tag06b.J));
}