//@execute 0=false; 1=false; 2=false; 3=false; 4=true; 5=false; 6=false
component tquery11 {
	def main(arg: int) -> bool {
		var v: void;
		if (arg == 1) return query(0);
		if (arg == 2) return query('0');
		if (arg == 3) return query(true);
		if (arg == 4) return query("false");
		if (arg == 5) return query(v);
		return false;
	}
	def query<T>(a: T) -> bool {
		return string.?(a);
	}
}
