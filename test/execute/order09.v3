//@execute 0=0; 1=1; 2=2; 3=21; 4=0

component order09 {
    var order: Array<int> = [0, 0];
    var pos: int = 0;

    def op(a: int, b: int) -> bool {
	return first(a) == second(b);
    }

    def first(a: int) -> int {
        order[pos] = 1;
        pos = pos + 1;
        return a;
    }

    def second(a: int) -> int {
        order[pos] = 2;
        pos = pos + 1;
        return a;
    }

    def main(arg: int) -> int {
	pos = 0;
        var result = op(1, 2);
	if (arg == 1) return order[0];
	if (arg == 2) return order[1];
	if (arg == 3) {
		if (result) return 11;
		else return 21;
	}
	return 0;
    }

}
