//@execute 0=true; 1=true; 2=true; 3=false
class closure01a<T>(a: T) {
	def eq(b: T) -> bool {
		return a == b;
	}
}
def main(a: int) -> bool {
	if (a == 0) return eq<bool>(true)(true);
	if (a == 1) return eq<byte>('1')('1');
	if (a == 2) return eq<int>(1)(1);
	return false;
}
def eq<T>(a: T) -> T -> bool {
	return closure01a.new(a).eq;
}
