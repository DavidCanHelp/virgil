//@execute 0=1
component tuple_zip06 {
	def main(arg: int) -> int {
		var r = zip([2], [-1]);
		return sum(r);
	}
	def zip<A, B>(a: Array<A>, b: Array<B>) -> Array<(A, B)> {
		var r = Array<(A, B)>.new(a.length);
		for (i = 0; i < r.length; i = i + 1) {
			r[i] = (a[i], b[i]);
		}
		return r;
	}
	def sum(a: Array<(int, int)>) -> int {
		var sum = 0;
		for (i = 0; i < a.length; i = i + 1) {
			sum = sum + a[i].0 + a[i].1;
		}
		return sum;
	}
}
