component int_op01 {
	def main(arg: int) -> bool {
		if (arg == 1) return int.+ == int.+;
		if (arg == 2) return int.- == int.-;
		if (arg == 3) return int.* == int.*;
		if (arg == 4) return int./ == int./;
		if (arg == 5) return int.% == int.%;
		if (arg == 6) return int.& == int.&;
		if (arg == 7) return int.| == int.|;
		if (arg == 8) return int.^ == int.^;
		if (arg == 9) return int.< == int.<;
		if (arg == 10) return int.<= == int.<=;
		if (arg == 11) return int.> == int.>;
		if (arg == 12) return int.>= == int.>=;
		return false;
	}
}
