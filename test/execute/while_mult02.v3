//@execute (0,0,0)=0; (1,1,1)=3; (2,3,4)=9; (1,5,6)=12
component while_mult02 {
	def main(a: int, b: int, c: int) -> int {
		var i = 0, j = 0, k = 0;
		var sum = 0;
		while (i < a) {
			i = i + 1;
			sum = i;
		}
		while (j < b) {
			j = j + 1;
			sum = i + j;
		}
		while (k < c) {
			k = k + 1;
			sum = i + j + k;
		}
		return sum;
	}
}
