type vt_array09a<DDDDD> { case A; case B; case C; case D; case E; }
def test<X>() -> bool {
	def x = [vt_array09a<X>.A, vt_array09a<X>.B, vt_array09a<X>.C, vt_array09a<X>.D, vt_array09a<X>.E];
	for (i = 0; i < x.length; i++) {
		if (x(i).tag != i) return false;
	}
