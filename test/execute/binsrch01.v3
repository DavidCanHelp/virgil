//@execute 0='a'; 1='b'; 2='c'; 3='d'; 4='e'; 5='f'; 6='g'; 7='h'; 8='h'
component binsrch01 {
	def main(a: int) -> byte {
		var x = byte.!(a);
		if (x < 4) {
			if (x < 2) {
				if (x < 1) return 'a';
				else return 'b';
			} else {
				if (x < 3) return 'c';
				else return 'd';
			}
		} else {
			if (x < 6) {
				if (x < 5) return 'e';
				else return 'f';
			} else {
				if (x < 7) return 'g';
				else return 'h';
			}
		}
	}
}
