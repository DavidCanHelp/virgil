//@execute (11,0)=11; (11,1)=5; (0,32)=0; (100,32)=0; (-200,33)=-1; (65536, 100)=0; (-200,-10)=-1
def main(a: int, b: int) -> int {
	if (b == 0) return a >> 0;
	if (b == 1) return a >> 1;
	if (b == 100) return a >> 100;
	if (b == 33) return a >> 33;
	if (b == 32) return a >> 32;
	if (b == -10) return a >> byte.!(-10);
	return a >> byte.!(b);
}

