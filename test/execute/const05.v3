//@execute 0=0; 1=255; 2=250; 3=0; 4=13; 5=128; 6=129; 7=255
component const05 {
	var a: byte = byte.!(0);
	var b: byte = byte.!(-1);
	var c: byte = byte.!(250);
	var d: byte = byte.!(256);
	var e: byte = byte.!(13);
	var f: byte = byte.!(128);
	var g: byte = byte.!(-127);
	var h: byte = byte.!(511);

	def main(x: int) -> int {
		if (x == 0) return a;
		if (x == 1) return b;
		if (x == 2) return c;
		if (x == 3) return d;
		if (x == 4) return e;
		if (x == 5) return f;
		if (x == 6) return g;
		return h;
	}
}
