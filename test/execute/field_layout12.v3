//@execute 0=-32; 1=1500; 2=2005555; 3=-1000000000; 4=42; 11=42
class field_layout12a {
	var a: int;
	var b: int;
	var c: int;
	var d: int;
}

component field_layout12 {
	def main(w: int) -> int {
		var o = field_layout12a.new();
		o.a = -32;
		o.b = 1500;
		o.c = 2005555;
		o.d = -1000000000;
		for (i = 10; i < w; i = i + 1) {
			o.a = o.b = o.c = o.d = 0;
		}
		match(w) {
			0: return o.a;
			1: return o.b;
			2: return o.c;
			3: return o.d;
		}
		return 42;
	}
}
