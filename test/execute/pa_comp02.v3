//@execute 0=0; 3=3; 7=7
component pa_comp02 {
	var f: int;
	def main(a: int) -> int {
		f = 0;
		var m = pa_comp02a.m(1, 2, _);
		for (i = 0; i < a; i++) {
			m(3);
		}
		return f;
	}
}
component pa_comp02a {
	var g: int;
	var h: int;
	def m(i: int, j: int, k: int) {
		pa_comp02.f = pa_comp02.f + i;
		g = g + j;
		h = h + k;
	}
}