//@execute 0=!DivideByZeroException; 1=49; 2=11; 3=66
def main(a: int) -> int {
	if (a == 0) return m(0);
	if (a == 1) return m('1');
	if (a == 2) return m(true);
	return m("");
}
def m<T>(a: T) -> int {
	if (int.?(a)) return 1 / int.!(a);
	if (byte.?(a)) return byte.!(a);
	if (bool.?(a)) return if (bool.!(a), 11, 12);
	return 66;
}
