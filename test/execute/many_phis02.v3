//@execute 0=0; 1=5; 2=10; 3=15; 4=20; 10=50; 11=45; 21=5
component many_phis02 {
	def main(a: int) -> int {
		var x0 = 0,  x1 = 0, x2 = 0, x3 = 0, x4 = 0, x5 = 0, x6 = 0, x7 = 0, x8 = 0, x9 = 0;
		for (i = 0; i < a; i = i + 1) {
			match (i % 10) {
				0: x0 = 5 - x0;
				1: x1 = 5 - x1;
				2: x2 = 5 - x2;
				3: x3 = 5 - x3;
				4: x4 = 5 - x4;
				5: x5 = 5 - x5;
				6: x6 = 5 - x6;
				7: x7 = 5 - x7;
				8: x8 = 5 - x8;
				9: x9 = 5 - x9;
			}
		}
		return x0 + x1 + x2 + x3 + x4 + x5 + x6 + x7 + x8 + x9;
	}
}
