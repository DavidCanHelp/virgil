//@execute 0=true; 1=false; 2=false; 3=true; 4=false
type vt_name08 {
	case ADD;
	case SUB;
}
def eq(a: vt_name08, b: vt_name08) -> bool {
	var x = a.name, y = b.name;
	for (i = 0; i < x.length; i++) if (x(i) != y(i)) return false;
	return true;
}
def main(a: int) -> bool {
	if (a == 0) return eq(vt_name08.ADD, vt_name08.ADD);
	if (a == 1) return eq(vt_name08.ADD, vt_name08.SUB);
	if (a == 2) return eq(vt_name08.SUB, vt_name08.ADD);
	if (a == 3) return eq(vt_name08.SUB, vt_name08.SUB);
	return false;
}
