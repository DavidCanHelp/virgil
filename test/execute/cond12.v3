//@execute (false, false)=32; (false, true)=1632; (true, false)=41120; (true, true)=61152
var result: int;
var x: bool;
var y: bool;
def main(a: bool, b: bool) -> int {
	result = 0;
	x = a;
	y = b;
	record(mx() & mx());
	record(mx() & my());
	record(mx() & true);
	record(mx() & false);
	record(my() & mx());
	record(my() & my());
	record(my() & true);
	record(my() & false);
	record(true & mx());
	record(true & my());
	record(true & true);
	record(true & false);
	record(false & mx());
	record(false & my());
	record(false & true);
	record(false & false);
	return result;
}
def record(a: bool) {
	if (a) result = result << 1 | 1;
	else result = result << 1;
}
def mx() -> bool { return x; }
def my() -> bool { return y; }

