//@execute 0=0
class zscope03a {
	var envDepth: int;
	var envStack: Array<int> = [0, 1];
}
component zscope03 {
	def main(a: int) -> int {
		var o = zscope03a.new();
		enterScope(o);
		return o.envStack[0];
	}
	def grow(a: Array<int>, len: int) -> Array<int> {
		return a;
	}
	def enterScope(o: zscope03a) {
		o.envStack = grow(o.envStack, o.envStack.length * 2);
	}
}
