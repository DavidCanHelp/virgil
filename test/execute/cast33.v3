//@execute 0=42; 1=!TypeCheckException; 2=48; 3=!TypeCheckException; 4=!TypeCheckException; 5=42
def main(arg: int) -> int {
	var v: void;
	if (arg == 1) return int.view<bool>(true);
	if (arg == 2) return int.view<byte>('0');
	if (arg == 3) return int.view<string>("a");
	if (arg == 4) return int.view<void>(v);
	return int.!(42);
}
