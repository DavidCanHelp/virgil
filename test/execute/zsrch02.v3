//@execute 0=2; 1=1; 2=1; 3=2; 4=3; 7=2; 9=3
var array: Array<Array<int>> = [[0, 1], [1, 2, -1], [3, 2, 7, 8]];
def main(a: int) -> int {
	var count = 0;
	for (i < array.length) {
		var e = array(i);
		for (j < e.length) {
			if (e(j) == a) {
				count--;
				break;
			}
		}
		count++;
	}
	return count;
}

