//@execute 0=12; 1=12
def table: Array<int> = [0, 1, 1, 2, 
			   1, 2, 2, 3,
			   1, 2, 2, 3,
			   2, 3, 3, 4];
def main(var a: int) -> int {
	for (i = 0; i < 16; i = i + 1) {
		var result = 0, x = i;
		for (j = 0; j < 4; j = j + 1) {
			if ((x & 1) == 1) result = result + 1;
			x = x >>> 1;
		}
		if (table(i) != result) return 42;
	}
	return 12;
}
