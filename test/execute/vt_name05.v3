//@execute 0=true; 1=true; 2=false
type vt_name05 {
	case ADD;
	case SUB;
}
def eq(a: vt_name05, b: vt_name05) -> bool {
	var x = vt_name05.name(a), y = vt_name05.name(b);
	for (i = 0; i < x.length; i++) if (x(i) != y(i)) return false;
	return true;
}
def main(a: int) -> bool {
	if (a == 0) return eq(vt_name05.ADD, vt_name05.ADD);
	if (a == 1) return eq(vt_name05.SUB, vt_name05.SUB);
	return false;
}
