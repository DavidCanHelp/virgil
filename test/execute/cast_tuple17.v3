def cast<A, B>(a: A) -> B {
	return B.!<A>(a);
}
def main() -> int {
	var x = cast<(int, int), (int, void)>((1, 99)).0;
	var y = cast<(int, int), (void, int)>((98, 2)).1;
	var z = cast<(int, void), (int, void)>((4, ())).0;
	var w = cast<(void, int), (void, int)>(((), 8)).1;
	return x + y + z + w;
}
