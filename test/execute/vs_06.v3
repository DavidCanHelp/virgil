//@execute 0=6; 1=2; 2=5
class vs_06<T> {
	def m() -> int { return 1; }
	def n() -> int { return Array<T>.new(2).length; }
	def o() -> int { return 3; }
}
def main(a: int) -> int {
	match (a) {
		0: {
			var o = vs_06<byte>.new();
			return o.m() + o.n() + o.o();
		} 
		1: {
			var o = vs_06<bool>.new();
			return o.n();
		}
	} else {
		var o = vs_06<int>.new();
		return o.n() + o.o();
	}
}
