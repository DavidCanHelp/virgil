//@execute 0=3; 1=4; 2=!NullCheckException
class inline_get01 {
	var f: int;
	new(f) { }
}
def getf(o: inline_get01) -> int {
	return o.f;
}
def get2(o: inline_get01) -> int {
	return getf(o);
}
var x = [inline_get01.new(3), inline_get01.new(4), null];
def main(a: int) -> int {
	return get2(x(a));
}
