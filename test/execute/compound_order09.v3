//@execute 0=42; 7=49; 49=91
def main(a: int) -> int {
	clear(a);
	LV(1)[I(2)] += V(3);
	return storage[0];
}
var serial = 1;
var storage = [0];
def clear(start: int) {
	serial = 1;
	storage(0) = start;
}
def LV(x: int) -> Array<int> {
	if (serial++ != x) return Array.new(1 / 0);
	return storage;
}
def I(x: int) -> int {
	if (serial++ != x) return 1 / 0;
	return 0;
}
def V(x: int) -> int {
	if (serial++ != x) return 1 / 0;
	return 42;
}
