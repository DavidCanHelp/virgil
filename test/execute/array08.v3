component array08 {
    var a: Array<bool> = Array<bool>.new(1);
    var b: Array<bool> = Array<bool>.new(3);

    def main(arg: int) -> bool {
	if (arg == 1) return a(0);
	if (arg == 2) return b(0);
	if (arg == 3) return b(1);
	if (arg == 4) return b(2);
	return true;
    }
}
