//@execute (1, 2, 3)=171; (4, -5, 6)=153
component many_phis10 {
	def main(a: int, b: int, c: int) -> int {
		var x: int, y: int, z: int, sum: int;
		for (i = 0; i < 28; i++) {
			match(i) {
				0:  { x = a; y = a; z = a; }
				1:  { x = a; y = a; z = b; }
				2:  { x = a; y = a; z = c; }
				3:  { x = a; y = b; z = a; }
				4:  { x = a; y = b; z = b; }
				5:  { x = a; y = b; z = c; }
				6:  { x = a; y = c; z = a; }
				7:  { x = a; y = c; z = b; }
				8:  { x = a; y = c; z = c; }
				9:  { x = b; y = a; z = a; }
				10: { x = b; y = a; z = b; }
				11: { x = b; y = a; z = c; }
				12: { x = b; y = b; z = a; }
				13: { x = b; y = b; z = b; }
				14: { x = b; y = b; z = c; }
				15: { x = b; y = c; z = a; }
				16: { x = b; y = c; z = b; }
				17: { x = b; y = c; z = c; }
				18: { x = c; y = a; z = a; }
				19: { x = c; y = a; z = b; }
				20: { x = c; y = a; z = c; }
				21: { x = c; y = b; z = a; }
				22: { x = c; y = b; z = b; }
				23: { x = c; y = b; z = c; }
				24: { x = c; y = c; z = a; } 
				25: { x = c; y = c; z = b; }
				26: { x = c; y = c; z = c; }
			}
			sum = sum + x + y + z;
		}
		return sum;
	}
}
