component const01 {
    var a: int = 1;
    var b: int = -1;
    var c: int = 65536;
    var d: int = -200000;
    var e: int = -2147483648;

    def main(arg: int) -> bool {
	if (arg == 1) return a == 1;
	if (arg == 2) return b == -1;
	if (arg == 3) return c == 65536;
	if (arg == 4) return d == -200000;
	if (arg == 5) return e == -2147483648;
	return false;
    }
}
