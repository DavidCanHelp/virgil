//@execute 5=80; 6=96; 10=160
component while16 {
	def main(a: int) -> int {
		var x = 0;
		var i = 0;
		while (true) {
			if (i == 2) break;
			var j = 0;
			while (true) {
				if (j == 2) break;
				var k = 0;
				while (true) {
					if (k == 2) break;
					var l = 0;
					while (true) {
						if (l == 2) break;
						var m = 0;
						while (true) {
							if (m == a) break;
							x = x + 1;
							m = m + 1;
						}
						l = l + 1;
					}
					k = k + 1;
				}
				j = j + 1;
			}
			i = i + 1;
		}
		return x;
	}
}
