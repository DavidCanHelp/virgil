//@execute (0,0,0)=0; (1,1,1)=2; (2,3,4)=8; (1,5,6)=11
component for_mult03 {
	def main(a: int, b: int, c: int) -> int {
		var i = 0, j = 0, k = 0;
		var sum = 0;
		for (xx = 0; i < a; i = i + 1) {
			if (i == 0) {
				var x = 1;
				x = 0;
				i = i + x;
			} else {
				var y = 1;
				y = 0;
				i = i - y;
			}
			sum = i;
		}
		for (yy = 0; j < b; j = j + 1) {
			if (j == 0) {
				var x = 1;
				x = 0;
				i = i + x;
			} else {
				var y = 1;
				y = 0;
				i = i - y;
			}
			sum = i + j;
		}
		for (zz = 0; k < c; k = k + 1) {
			if (k == 0) {
				var x = 1;
				x = 0;
				i = i + x;
			} else {
				var y = 1;
				y = 0;
				i = i - y;
			}
			sum = i + j + k;
		}
		return sum;
	}
}
