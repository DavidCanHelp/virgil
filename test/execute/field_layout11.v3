//@execute false=16; true=16
class field_layout11a {
	var c0: byte = '0';
	var i0: int = 10;

	var c1: byte = '1';
	var i1: int = 11;

	var c2: byte = '2';
	var i2: int = 12;

	var c3: byte = '3';
	var i3: int = 13;

	def inc(v: int) {
		c0 = byte.!(c0 + v);
		c1 = byte.!(c1 + v);
		c2 = byte.!(c2 + v);
		c3 = byte.!(c3 + v);

		i0 = byte.!(i0 + v);
		i1 = byte.!(i1 + v);
		i2 = byte.!(i2 + v);
		i3 = byte.!(i3 + v);
	}
	def sum() -> int {
		return c3 + c2 - c1 - c0 + i3 + i2 - i1 - i0;
	}
}
var f: field_layout11a = field_layout11a.new();
def main(n: bool) -> int {
	var o = f;
	if (n) o = field_layout11a.new();
	o.inc(2);
	var s1 = o.sum();
	o.inc(-2);
	var s2 = o.sum();
	return s1 + s2;
}
