//@execute =54
def cases: Array<(u53, double)> = [
	(0, 0d),
	(1, 1d),
	(3, 3d),
	(123, 123d),
	(567, 567d),
	(1000000, 1000000d),
	(4194303, 4194303d),
	(4194304, 4194304d),
	(8388607, 8388607d),
	(8388608, 8388608d),
	(16777215, 16777215d),
	(2147483647, 2147483647d),
	(2147483648u, 2147483648d),
	(4294967295u, 4294967295d),
	(9007199254740991, 9007199254740991d)
];
def main() -> int {
	for (i < cases.length) {
		var t = cases[i];
		var x = t.1;
		var a: double = t.0;
		if (x != a) return -3 - i;
	}
	return 54;
}
