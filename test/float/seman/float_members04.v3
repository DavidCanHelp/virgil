var y: float;

def x1: float -> float = float.sqrt;
def z1: float = float.sqrt(y);

def x2: (float, float) -> float = float.pow;
def z2: float = float.pow(y, y);

def x3: float -> float = float.log;
def z3: float = float.log(y);

def x4: float -> float = float.exp;
def z4: float = float.exp(y);
