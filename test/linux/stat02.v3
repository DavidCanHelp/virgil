//@execute = 46
def SYS_STAT = 106;
def stat: Array<int> = Array.new(32);
def main() -> int {
	if (doStat(Linux.syscall(SYS_STAT, _)) != 0) return -1;
	return stat[5];
}
def doStat(f: (Pointer, Pointer) -> (int, int)) -> int {
	var path = Pointer.atContents("test.txt\x00");
	return f(path, Pointer.atContents(stat)).0;
}
