class object {
  var name: Array<byte>;
}

class ht_link {
  var obj: object;
  var link: ht_link;
}

class hashtable {
 
  var size: int;
  var table: Array<ht_link>;

  def app(f: object -> void) {
     var cntr = 0;
     while (cntr < size) {
       var head = table(cntr);
       while (head != null) {
         f(head.obj);
         head = head.link;
       }
     cntr = cntr + 1;
     }
      
  }
}

