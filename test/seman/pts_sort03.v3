def test() {
	var a = [0, 21, 2, 34, 5, 6, 68, -11, -90];
	sort(a, int.>);
	var b = "this is an odd string: ^&*@^*&";
	sort(b, byte.>);
}
def sort<T>(a: Array<T>, gt: (T, T) -> bool) {
	var i = 0, j = 0, len = a.length;
	while (i < len) {
		var j = i + 1;
		while (j < len) {
			if (gt(a(i), a(j))) {
				var t = a(i);
				a(i) = a(j);
				a(j) = t;
			}
			j = j + 1;
		}
		i = i + 1;
	}
}
