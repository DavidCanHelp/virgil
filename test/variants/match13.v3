//@execute 0=1; 1=2; 3=42
type HH { case A; case B; case C; case D; }
def all = [HH.A, HH.B, HH.C, HH.D, HH.D, HH.D];
def main(arg: int) -> int {
	while (true) {
		match(all[arg]) {
			A: return 1;
			B: {
				match(all[arg + 3]) {
					D: return 2;
				} else {
					break;
				}
			}
		} else {
			break;
		}
	}
	return 42;
}

