//@execute (0,0)=15; (0,1)=9; (0,2)=9; (0,3)=9; (1,1)=15; (1,2)=9; (1,3)=9; (2,2)=15; (2,3)=9; (3,3)=15
type v05 {
	case X(i: int);
	case Y(i: int);
}
def compare(x: v05, y: v05) -> int {
	return
		if(x == x, 0b0001) |
		if(x == y, 0b0010) |
		if(y == x, 0b0100) |
		if(y == y, 0b1000);
}
def xx = [v05.X(11), v05.Y(11), v05.X(12), v05.Y(12)];
def main(a: int, b: int) -> int {
	return compare(xx[a], xx[b]);
}
