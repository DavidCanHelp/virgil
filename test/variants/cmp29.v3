//@execute 0=1; 1=2; 2=4; 3=8; 4=16
type A {
	case X(v: int);
	case Y(v: int);
}

def d: A;
def vals = [d, A.X(3), A.X(4), A.Y(3), A.Y(4)];

def eq(v: A) -> int {
	var result = 0;
	for (i < vals.length) {
		if (v == vals(i)) result |= (1 << byte.!(i));
	}
	return result;
}

def main(a: int) -> int {
	return eq(vals(a));	
}
