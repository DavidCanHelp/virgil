//@execute 0=12; 1=14
class A { def m = 6; }
class B { def m = 7; }
type T {
	case isA(a1: A, a2: A);
	case isB(b1: B, b2: B);
}
var x = [T.isA(A.new(), A.new()), T.isB(B.new(), B.new())];
def main(a: int) -> int {
	match (x[a]) {
		isA(a1, a2): return a1.m + a2.m;
		isB(b1, b2): return b1.m + b2.m;
	}
}
