type array07a<T> { case A; case B; }
def test<X>() -> bool {
	def A = array07a<X>.A, B = array07a<X>.B;
	def x = [A, B, A, B, A, B];
	return
		x(0) == A &&
		x(1) == B &&
		x(2) == A &&
		x(3) == B &&
		x(4) == A &&
		x(5) == B;
}
def main() -> bool {
	return test<void>() && test<int>() && test<string>();
}
