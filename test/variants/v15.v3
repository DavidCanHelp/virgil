//@execute 0=true; 1=true; 2=true; 3=false
type v15 {
	case X(i: int);
	case Y(i: int);
}
def compare(x: v15, y: v15) -> bool {
	return x == y;
}
def main(a: int) -> bool {
	def d: v15, x = v15.X(12), y = v15.Y(13);
	match(a) {
		0 => return compare(d, d);
		1 => return compare(x, x);
		2 => return compare(y, y);
	}
	return false;
}
