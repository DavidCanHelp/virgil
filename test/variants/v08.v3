//@execute (0,0)=0; (0,1)=6; (0,2)=6; (0,3)=6; (1,1)=0; (1,2)=6; (1,3)=6; (2,2)=0; (2,3)=6; (3,3)=0
type v08<Y> {
	case X(i: Y);
	case Y(i: Y);
}
def compare<C>(x: v08<C>, y: v08<C>) -> int {
	return
		if(x != x, 0b0001) |
		if(x != y, 0b0010) |
		if(y != x, 0b0100) |
		if(y != y, 0b1000);
}
def xx = [v08.X(11), v08.Y(11), v08.X(12), v08.Y(12)];
def main(a: int, b: int) -> int {
	return compare(xx(a), xx(b));
}
