//@execute 0=2; 1=2; 3=13
type List<T> {
	case None;
	case Cons(head: T, tail: List<T>);
}
def len<T>(var l: List<T>) -> int {
	var r = 0;
	while (true) {
		match (l) {
			None: return r;
			Cons(head, tail): { r++; l = tail; }
		}
	}
	return r;
}
var x = List.Cons(4, List.Cons(3, List<int>.None));
var y = List.Cons("f", List.Cons("t", List<string>.None));
def main(a: int) -> int {
	if (a == 0) return len(x);
	if (a == 1) return len(y);
	return 13;
}
