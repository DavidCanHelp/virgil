type F<T> {
	case None;
	case M(g: T);
}
type G {
	case None;
	case M(x: int, y: string);
}
type H {
	case None;
	case M(x: byte, y: bool);
}
var s = "this is a string";
var g = G.M(3, s);
var h = H.M('4', true);
var x = [F.M(g), F.M(g)];
var y = [F.M(h), F.M(h)];
def main() -> int {
	var sum = 0;
	match(x(0)) {
		None: sum = 0;
		M(g): {
			match(g) {
				None: sum = 0;
				M(x, y): sum = sum + x + y.length;
			}
		}
	}
	match(y(0)) {
		None: sum = 0;
		M(h): {
			match(h) {
				None: sum = 0;
				M(x, y): sum = sum + x + if(y, 1, 0);
			}
		}
	}
	return sum;
}
