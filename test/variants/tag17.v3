//@execute = true
type T1 { case A; case B; case C; }
type T2 { case X; case Y; case Z; }
def check<T>(x: Array<T>, tag: T -> u2) -> bool {
	for (i < x.length) {
		if (tag(x(i)) != i) return false;
	}
	return true;
}
def main() -> bool {
	return check([T1.A, T1.B, T1.C], T1.tag) &&
		check([T2.X, T2.Y, T2.Z], T2.tag);
}
