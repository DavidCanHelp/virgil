def compute(f: M -> int) -> int {
	var d: M;
	return  (f(d)   << 0) |
		(f(M.A) << 2) |
		(f(M.B) << 4) |
		(f(M.C) << 6);
}

def main(a: int) -> int {
	match (a) {
		0 => return compute(M.m1);
		1 => return compute(M.m2);
		2 => return compute(M.m3);
		3 => return compute(M.m4);
		4 => return compute(M.m5);
		5 => return compute(M.m6);
		6 => return compute(M.m7);
		7 => return compute(M.m8);
	}
	return 0;
}
