//@execute 0=0; 1=0; 2=0; 3=3; 4=!BoundsCheckException
type A {
	case A(b: B);
	def m() -> int { match (this) { A(b): return b.m(); } }
}
type B {
	case B(c: C);
	def m() -> int { match (this) { B(c): return c.m(); } }
}
type C {
	case C(x: int);
	def m() -> int { match (this) { C(x): return x; } }
}
def a: A, b: B, c: C;
def x = [A.A(b), A.A(B.B(c)), A.A(B.B(C.C(0))), A.A(B.B(C.C(3)))];

def main(a: int) -> int {
	return x[a].m();
}
