//@execute 0=44; 2=44; 3=44; 88=44; -9=44; 8734873=44
//@heap-size=80000
type Link {
	case None;
	case Foo(l: Link);
	case Bar(l: Link);
}
def make(var a: int) -> Link {
	var link: Link;
	while (a != 0) {
		link = if ((a & 1) == 0, Link.Foo(link), Link.Bar(link));
		a >>>= 1;
	}
	return link;
}
def main(a: int) -> int {
	for (i < 32) {
		var b = a | (1 << byte.!(i));
		var eq1 = (a == b);
		var eq2 = (make(a) == make(b));
		if (eq1 != eq2) return -1;
	}
	return 44;
}