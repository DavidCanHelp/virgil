//@execute 0=true; 1=true; 2=true; 3=true; 4=false
type Data<T> {
	case Box(x: T);
	def m() -> T { match(this) { Box(x): return x; } }
}
def str = "fxxx";
def x = [Data.Box(11)];
def y = [Data.Box(true)];
def z = [Data.Box('t')];
def w = [Data.Box(str)];

def main(a: int) -> bool {
	if (a == 0) return x[0].m() == 11;
	if (a == 1) return y[0].m() == true;
	if (a == 2) return z[0].m() == 't';
	if (a == 3) return w[0].m() == str;
	return false;
}
