//@execute 0=6; 1=7; 2=7; 3=17; 4=33
type T {
	case M(x: int, y: int);
	def get() -> int { match(this) { M(x, y): return x + y; } }
}
def main(a: int) -> int {
	def A = (3, 3), B = (4, 3), C = (5, 2), D = (9, 8);
	if (a == 0) return T.M(A).get();
	if (a == 1) return T.M(B).get();
	if (a == 2) return T.M(C).get();
	if (a == 3) return T.M(D).get();
	return 33;
}
