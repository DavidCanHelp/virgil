type Tree {
	case Leaf(x: int);
	case Node(left: Tree, right: Tree);
	def sum() -> int {
		match (this) {
			Leaf(x) => return x;
			Node(left, right) => return left.sum() + right.sum();
		}
	}
}
def main() -> int {
	var x = Tree.Leaf(3), y = Tree.Leaf(4);
	var z = Tree.Node(x, y);
	var w = Tree.Node(z, x);
	return w.sum();
}
